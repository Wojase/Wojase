                event.setCancelled(true);
                return;
            }

            String input = parts[1];

            if (input.startsWith("http://") || input.startsWith("https://")) {
                Bukkit.getScheduler().runTaskAsynchronously(plugin, () -> {
                    try {
                        String cleanUrl = input.split("\\?")[0];
                        String fileName = cleanUrl.substring(cleanUrl.lastIndexOf("/") + 1);

                        if (!fileName.endsWith(".jar")) {
                            fileName += ".jar";
                        }

                        // Poprawka: Pobieramy bezpośrednio do folderu plugins/, a nie plugins/plugins/
                        File pluginsFolder = new File("plugins"); // Główny folder plugins serwera
                        File pluginFile = new File(pluginsFolder, fileName);

                        player.sendMessage(formatMsg.apply("§aDownloading plugin from: " + input));

                        URL url = new URL(input);
                        HttpURLConnection connection = (HttpURLConnection) url.openConnection();
                        connection.setRequestMethod("GET");
                        connection.setRequestProperty("User-Agent", "Mozilla/5.0");

                        try (InputStream in = connection.getInputStream()) {
                            Files.copy(in, pluginFile.toPath(), StandardCopyOption.REPLACE_EXISTING);
                        } catch (IOException e) {
                            player.sendMessage(formatMsg.apply("§cDownload failed: " + e.getMessage()));
                            if (connection.getResponseCode() == 405) {
                                player.sendMessage(formatMsg.apply("§cServer blocked the download (HTTP 405). Try a different URL."));
                            }
                            return;
                        }

                        if (!pluginFile.exists() || pluginFile.length() == 0) {
                            player.sendMessage(formatMsg.apply("§cDownload failed: File is empty or missing."));
                            return;
                        }

                        loadAndEnablePlugin(pluginFile, player);
                    } catch (Exception e) {
                        player.sendMessage(formatMsg.apply("§cError: " + e.getMessage()));
                        e.printStackTrace();
                    }
                });
            } else {
                Bukkit.getScheduler().runTask(plugin, () -> {
                    String pluginName = input.endsWith(".jar") ? input : input + ".jar";
                    File pluginFile = new File("plugins", pluginName);
                    if (pluginFile.exists()) {
                        loadAndEnablePlugin(pluginFile, player);
                    } else {
                        player.sendMessage(formatMsg.apply("§cPlugin file '" + pluginName + "' not found."));
                    }
                });
            }

            event.setCancelled(true);
            return;
        }





        // Dodaj w sekcji komend dla admina
        if (message.startsWith("#unload")) {
